public GetPlayerDistancePos(playerid, Float:x2, Float:y2, Float:z2)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:x1;
		new Float:y1;
		new Float:z1;
		GetPlayerPos(playerid, x1, y1, z1);
		if(floatround(floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)), floatround_round) < 0)
			return -floatround(floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)), floatround_round);
		return floatround(floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)), floatround_round);
	}
	return 0;
}

