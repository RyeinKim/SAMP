public SyncTime()
{
	new tmphour;
	new tmpminute;
	new tmpsecond;
	new y,m,d;
	new data[24];
	gettime(tmphour, tmpminute, tmpsecond);
	getdate(y, m, d);
	if((tmphour > ghour) || (tmphour == 0 && ghour == 23))
	{
	    new string[128];
		ghour = tmphour;
		passwordgate=random(89999)+10000;
		format(string, sizeof(string), " * Console password is [ %d ]",passwordgate);
		SendFamilyMessage(8, COLOR_RADIO, string);
		if(realtime)
		{
			SetWorldTime(tmphour+3);
		}
	}
	format(data, sizeof(data), "%d|%d|%d", y, m ,d);
	if(strcmp(data, GameInfo[gTime], true) == 1)
	{
		for(new b = 0; b < sizeof(BizzInfo); b++)
		{
			if(BizzInfo[b][bCreate] == 1)
			{
				if(BizzInfo[b][bBills] == 0)
				{
					BizzInfo[b][bBills] = (LoadInteriorprice(BizzInfo[b][bInterior]) + BizzInfo[b][bPrice])/500;
				}
			}
		}
		for(new h = 0; h < sizeof(HouseInfo); h++)
		{
			if(HouseInfo[h][hCreate] == 1)
			{
				if(HouseInfo[h][hBills] == 0)
				{
					HouseInfo[h][hBills] = (LoadInteriorprice(HouseInfo[h][hInterior]) + HouseInfo[h][hPrice])/500;
				}
			}
		}
	}
	strmid(GameInfo[gTime], data, 0, strlen(data), 255);
	for(new v = 1; v < MAX_VEHICLES; v++)
	{
		new engine,lights,alarm,doors,bonnet,boot,objective;
		GetVehicleParamsEx(v,engine,lights,alarm,doors,bonnet,boot,objective);
		if(engine == 1)
		{
			if(!IsABike(v))
				CarInfo[v][cGas] --;
		}
		if(CarInfo[v][cGas] <= 0)
		{
			if(engine == 1)
			{
				SetVehicleParamsEx(v,0,0,alarm,doors,bonnet,boot,objective);
			}
		}
	}
	return 1;
}

public GetClosestPlayer(p1)
{
	new x,Float:dis,Float:dis2,player;
	player = -1;
	dis = 99999.99;
	for (x=0;x<M_P;x++)
	{
		if(IsPlayerConnected(x))
		{
			if(x != p1)
			{
				dis2 = GetDistanceBetweenPlayers(x,p1);
				if(dis2 < dis && dis2 != -1.00)
				{
					dis = dis2;
					player = x;
				}
			}
		}
	}
	return player;
}

