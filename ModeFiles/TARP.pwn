//		    return 3;
	}
	return 0;
}

public RGBAToHex(r, g, b, a)
{
	return (r<<24 | g<<16 | b<<8 | a);
}

public ResetCarInfo(vehicleid)
{
	CarInfo[vehicleid][cCreate]=0;
	CarInfo[vehicleid][cModel]=0;
	CarInfo[vehicleid][cPosx]=0;
	CarInfo[vehicleid][cPosy]=0;
	CarInfo[vehicleid][cPosz]=0;
	CarInfo[vehicleid][cAngle]=0;
	CarInfo[vehicleid][cColor]=0;
	CarInfo[vehicleid][cColor2]=0;
	CarInfo[vehicleid][cLock]=0;
	CarInfo[vehicleid][cGas]=0;
	CarInfo[vehicleid][cMember] = 0;
	if(VNeon1[vehicleid])
	{
		DestroyObject(VNeon1[vehicleid]);
		VNeon1[vehicleid] = 0;
	}
	for(new i = 0; i < MAX_TUNING;i++)
	{
		CarInfo[vehicleid][cTuning][i] = 999;
		CarInfo[vehicleid][cTrunk][i] = 0;
	}
	for(new i = 0; i < MAX_TRUNK;i++)
	{
		CarInfo[vehicleid][cTrunk][i] = 0;
	}
	CarInfo[vehicleid][cTow] = 0;
	CarInfo[vehicleid][cHealth] = 0;
	strmid(CarInfo[vehicleid][cOwner], "None", 0, strlen("None"), 255);
	strmid(CarInfo[vehicleid][cLP], "None", 0, strlen("None"), 255);
	CarInfo[vehicleid][cVir] = 0;
	strmid(CarInfo[vehicleid][cTowname], "None", 0, strlen("None"), 255);
	CarInfo[vehicleid][cInt] = 0;
	CarInfo[vehicleid][cFoise] = 0;
    CarInfo[vehicleid][cRealHealth]=1000;
    CarInfo[vehicleid][CheckRepair]=0;
	CarInfo[vehicleid][cTrunkLock]=1;
	Vint[vehicleid] = 0;
	Nitroused[vehicleid]=false;
	NitroPower[vehicleid]=0;
	return 1;
}

