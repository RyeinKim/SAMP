public OnPlayerSaveInfo(playerid)
{
	if(IsPlayerConnected(playerid))
	{
 		if(gPlayerLogged[playerid])
   		{
   			if(PKIN[playerid] != 0) return 1;
  			PlayerInfo[playerid][pHealth] = PlayerHackHealth[playerid];
  			PlayerInfo[playerid][pArmour] = PlayerHackArmour[playerid];
  			PlayerInfo[playerid][pArmourUsed] = false;

			GetPlayerPos(playerid, PlayerInfo[playerid][pPos_x], PlayerInfo[playerid][pPos_y], PlayerInfo[playerid][pPos_z]);
			PlayerInfo[playerid][pInt] = GetPlayerInterior(playerid);
			PlayerInfo[playerid][pVirWorld] = GetPlayerVirtualWorld(playerid);
			for(new is = 0; is < 13; is++)
			{
				new sweapon, sammo;
				GetPlayerWeaponData(playerid, is, sweapon, sammo);
				if(sweapon != 0 && sammo != 0)
				{
				  	PlayerInfo[playerid][pAGun][0] = sweapon;
				  	PlayerInfo[playerid][pGAmm][0] = sammo;
					break;
				}
				else if(is == 12)
				{
				  	PlayerInfo[playerid][pAGun][0] = 0;
				  	PlayerInfo[playerid][pGAmm][0] = 0;
				}
			}
   		}
	}
	return 1;
}

public OpenPlayerGate(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		for(new mo = 0; mo < sizeof(MoveObjectInfo); mo++)
		{
			if(IsPlayerInRangeOfPoint(playerid, MoveObjectInfo[mo][moDistance], MoveObjectInfo[mo][moX], MoveObjectInfo[mo][moY], MoveObjectInfo[mo][moZ]) && GetPlayerVirtualWorld(playerid) == MoveObjectInfo[mo][moVirtualWorld] && PlayerInfo[playerid][pMember] == MoveObjectInfo[mo][moMember])
			{
				MoveDynamicObject(MoveObjectInfo[mo][moNumber], MoveObjectInfo[mo][moMx], MoveObjectInfo[mo][moMy], MoveObjectInfo[mo][moMz], MoveObjectInfo[mo][moSpeed], MoveObjectInfo[mo][moMrx], MoveObjectInfo[mo][moMry], MoveObjectInfo[mo][moMrz]);
				SetTimerEx("CloseGata", MoveObjectInfo[mo][moWtime], false, "i", mo);
				break;
			}
		}
	}
	return 1;
}

