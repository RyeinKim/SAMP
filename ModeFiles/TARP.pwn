public OnVehicleMod(playerid, vehicleid, componentid)
{
    new vehicleide = GetVehicleModel(vehicleid);
    new modok = islegalcarmod(vehicleide, componentid);
    if(!modok)
	{
		RemoveVehicleComponent(vehicleid,componentid);
		new string[128],sip[100];
		GetPlayerIp(playerid,sip, sizeof(sip));
		format(string,sizeof(string),"서버: %s(%d)의 차량 튜닝 핵을 감지 하였습니다.[IP:%s]",GetName(playerid),playerid,sip);
//		printf(string);
		for(new i = 0; i < M_P; i++)
		{
			if(IsPlayerConnected(i))
			{
				if(PlayerInfo[i][pRealadmin] >= 1 && CheckHackMessage[i] == 0)
				{
					SafeSendClientMessage(i, 0xFFFFFFAA, string);
				}
			}
		}
		return 0;
    }
	TunningTimer[playerid]=SetTimerEx("TunningMoney",1000,false,"i",playerid);
	return 1;
}

