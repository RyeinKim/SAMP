				DutyColor[playerid]=TEAM_HIT_COLOR;
				PlayerInfo[playerid][pRequestingBackup] = 0;
			}
			else
			{
				if(calledbytimer != 1)
				{
					SafeSendClientMessage(playerid, COLOR_DARKNICERED, "당신은 지원요청을 하지 않았습니다.");
