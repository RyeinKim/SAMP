stock SetVarString(const Name[], var[])
{
	new b = INVALID_VAR_ID;
	if (!strlen(Name)) return b;
	for (new a = 0; a < MAX_VARS; a++)
	{
	    if (!strcmp(VarName[a], Name, true) && strlen(VarName[a]))
	    {
			SetGVarString(Name, var);
			SaveVar(a);
			return a;
		}
		else if (!strlen(VarName[a])) b = a;
	}
	if (b == INVALID_VAR_ID) return b;
	strmid(VarName[b], Name, 0, 256, 256);
	SetGVarString(Name, var);
	SaveVar(b);
	return b;
}

stock SetVarInt(const Name[], var)
{
	new str[256];
	format(str, sizeof(str), "%d", var);
	return SetVarString(Name, str);
}

stock SetVarFloat(const Name[], Float:var)
{
	new str[256];
	format(str, sizeof(str), "%f", var);
	return SetVarString(Name, str);
}

stock GetVarString(const Name[])
{
	new a[256];
	GetGVarString(Name, a);
	return a;
}
