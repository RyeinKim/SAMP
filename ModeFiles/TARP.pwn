					if(PlayerInfo[i][pDeath] == 1)
					{
						SafeSendClientMessage(i,0xFC595AFF,  "이제 (/죽음)으로 새로운 삶을 시작하거나 구조가 될때까지 기달릴 수 있습니다.");
						SafeSendClientMessage(i,0xFC595AFF,  "(/죽음)을 하면 이전 기억은 잃어 버리게 되는 상황이 됩니다 (기억할 시 법률위반 사항으로 처벌당할 수 있습니다)");
					}
				}
				if(PlayerHackHealth[i] == 0.0)
				{
					SafeSetPlayerHealth(i, 50);
				}
				else if(health > PlayerHackHealth[i] && GetPlayerState(i) != PLAYER_STATE_NONE && GetPlayerState(i) != PLAYER_STATE_NONE && GetPlayerState(i) != PLAYER_STATE_SPAWNED && rugmod[i]==false)
				{
					SafeSetPlayerHealth(i, PlayerHackHealth[i]);
				}
				else if(health < PlayerHackHealth[i] && rugmod[i]==false)
				{
				    if(health!=0)
				    {
						PlayerHackHealth[i] = health;
					}
				}
				else if(health > 100 && rugmod[i]==false)
				{
					SafeSetPlayerHealth(i,100);
				}
				if(armour > PlayerHackArmour[i] && GetPlayerState(i) != PLAYER_STATE_NONE && GetPlayerState(i) != PLAYER_STATE_NONE && GetPlayerState(i) != PLAYER_STATE_SPAWNED)
				{
					SafeSetPlayerArmour(i, PlayerHackArmour[i]);
				}
				else if(armour < PlayerHackArmour[i])
				{
				    if(armour!=0)
				    {
						PlayerHackArmour[i] = armour;
					}
				}
				else if(armour > 100)
				{
					SafeSetPlayerArmour(i,100);
				}
				if(armour == 0 && PlayerInfo[i][pArmourUsed]==true)
				{
				    PlayerInfo[i][pArmourUsed]=false;
				    PlayerInfo[i][pArmour]=0;
				}
				if(PlayerCuffed[i] == 1)
				{
					if(PlayerCuffedTime[i] > 0 && PlayerCuffedTime[i] != 9999)
					{
						PlayerCuffedTime[i] --;
						if(PlayerCuffedTime[i] == 0)
						{
							TogglePlayerControllable(i, 1);
							PlayerCuffed[i] = 0;
							PlayerCuffedTime[i] = 0;
							PlayerTazeTime[i] = 1;
						}
					}
				}
				if(CellTime[i] > 0)
				{
					if(CellTime[i] == cchargetime)
					{
						CellTime[i] = 1;
						if(Mobile[i] < M_P)
						{
							if(Mobile[Mobile[i]] == i)
							{
								CallCost[i] = CallCost[i]+callcost;
							}
						}
					}
					CellTime[i] ++;
				}
				if(MineWorkDelay[i])
				    MineWorkDelay[i] = false;
				if(PlayerTazeTime[i] >= 1)
				{
					PlayerTazeTime[i] += 1;
					if(PlayerTazeTime[i] == 15)
					{
						PlayerTazeTime[i] = 0;
					}
				}
				if(PlayerInfo[i][pSec] < 60)
				{
					PlayerInfo[i][pSec] ++;
				}
				else
				{
					PlayerInfo[i][pSec] = 0;
					PlayerInfo[i][pMin] ++;
				}
				if(PlayerInfo[i][pMin] >= 60)
				{
					PlayerInfo[i][pLevel] ++;
					PlayerInfo[i][pMin] = 0;
					PayDay(i);
				}
				if(Gundel[i] > 0)
				{
					Gundel[i] --;
				}
				if(Delay[i] > 0)
				{
					Delay[i] --;
				}
				if(PlayerInfo[i][pDtime] > 0)
				{
					if(PlayerInfo[i][pDtime] % 16 == 0)
					{
						GivePlayerHealth(i, 10);
					}
					PlayerInfo[i][pDtime] --;
					if(PlayerInfo[i][pDtime] == 0)
					{
						SetPlayerWeather(i, Weather);
					}
				}
				if(HireCar[i] != 0 && Ttime[i] > 0)
				{
					Ttime[i] --;
				}
				else if(HireCar[i] != 0)
				{
					Ttime[i] = 0;
					if(HireCarType[i] == 1)
						DestroyVehicle(HireCar[i]);
					else if(HireCarType[i] == 2)
						SetVehicleToRespawn(HireCar[i]);
					HireCar[i] = 0;
					HireCarType[i] = 0;
				}
				if(PlayerInfo[i][pJobTime] > 0)
				{
					PlayerInfo[i][pJobTime]--;
				}
				if(NewsCool[i] > 0)
				{
					NewsCool[i]--;
				}
				if(NewbieTimer[i] > 0)
				{
					NewbieTimer[i] --;
				}
			}
		}
	}
	return 1;
}

