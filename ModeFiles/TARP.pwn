				SetPlayerFacingAngle(playerid,CarParkInfo[cp][cpA]);
				SetPlayerVirtualWorld(playerid, CarParkInfo[cp][cpVir]);
				SetPlayerInterior(playerid, CarParkInfo[cp][cpInt]);
				return 1;
			}
