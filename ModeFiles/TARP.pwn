					}
				}
				if(calledbytimer != 1)
				{
					SafeSendClientMessage(playerid, TEAM_BLUE_COLOR, "지원요청이 취소되었습니다.");
				}
				else
				{
					SafeSendClientMessage(playerid, TEAM_BLUE_COLOR, "당신의 지원요청이 자동적으로 취소되었습니다.");
