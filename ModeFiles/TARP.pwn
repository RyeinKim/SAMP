public OnPlayerEnterCheckpoint(playerid)
{
	new vehicleid = GetPlayerVehicleID(playerid);
	
	if(NomalCheckpoint[playerid])
	{
		if(shovel[playerid] == true)
		{
			if(PlayerInfo[playerid][pJob] == 1)
			{
				for(new cp = 0; cp < sizeof(RhkdtksPos); cp++)
				{
					if(IsPlayerInRangeOfPoint(playerid, 10.0, RhkdtksPos[cp][0], RhkdtksPos[cp][1], RhkdtksPos[cp][2]))
					{
						return 1;
					}
				}
			}
		}
		else if( PlayerSweepJob[playerid] == true )
		{
			if(PlayerInfo[playerid][pJob] == 10)
			{
				if( IsPlayerInAnyVehicle(playerid) && GetVehicleModel(vehicleid) == 574)
				{
					for(new cp = 0; cp < sizeof(SweepPos); cp++)
					{
						if(IsPlayerInRangeOfPoint(playerid, 5.0, SweepPos[cp][0], SweepPos[cp][1], SweepPos[cp][2]))
						{
							PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
							if(cp == sizeof(SweepPos)-1)
							{
								new pay, cool;
								pay = GetVarInt("청소부임금");
								cool = GetVarInt("청소부쿨타임");
								NomalCheckpoint[playerid] = false;
								DisablePlayerCheckpoint(playerid);
								PlayerSweepJob[playerid] = false;
								PlayerInfo[playerid][pJobTime] = cool; // 원래 쿨타임 1200초
								new string[128];
								format(string, sizeof(string), "(정보) :"#C_WHITE" 청소부 일을 끝마쳤습니다. 총 수익은 $%d이고, 쿨타임은 %d입니다.", pay,cool);
								SafeSendClientMessage(playerid, COLOR_YELLOW, string);
							//	SafeSendClientMessage(playerid,COLOR_YELLOW, "석가 탄신일을 맞이하여 나라에서 50$ 더 주었습니다.");
								SafeGivePlayerMoney(playerid, pay); // 원래 350임금
								SetVehicleToRespawn(HireCar[playerid]);
								HireCar[playerid] = 0;
								HireCarType[playerid] = 0;
								return 1;
							}
							SetPlayerCheckpoint(playerid, SweepPos[cp+1][0], SweepPos[cp+1][1], SweepPos[cp+1][2], 5.0);
							return 1;
						}
					}
				} else return SendClientErrorMessage(playerid, "환경미화원 차량에 탑승하여 주시기 바랍니다 ");
			}
		}
		else if(FarmerStart[playerid] == true)
		{
			if(PlayerInfo[playerid][pJob] == 2)
			{
				if(GetVehicleModel(vehicleid) == 532)
				{
					if(IsPlayerInRangeOfPoint(playerid, 5.0, -376.8234,-1426.6041,25.7266))
					{
						new cool = GetVarInt("밀KG");
						PlayerInfo[playerid][pWheat] += cool;
						PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
						new randch = random(sizeof(FarmerCheckPoint));
						SetPlayerCheckpoint(playerid, FarmerCheckPoint[randch][0], FarmerCheckPoint[randch][1], FarmerCheckPoint[randch][2], 5.0);
						new string[128];
						format(string, sizeof(string), "(정보) :"#C_WHITE" 밀을 수확하여 %dKG의 량을 얻었습니다.",cool);
						SafeSendClientMessage(playerid, COLOR_YELLOW, string);
					}
					else
					{
						for(new ch = 0; ch < sizeof(FarmerCheckPoint); ch++)
						{
							if(IsPlayerInRangeOfPoint(playerid, 10.0, FarmerCheckPoint[ch][0], FarmerCheckPoint[ch][1], FarmerCheckPoint[ch][2]))
							{
								PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
								SafeSendClientMessage(playerid,COLOR_YELLOW,"밀을 수확합니다, 잠시만 기다리세요.");
								TogglePlayerControllable(playerid, 0);
								DisablePlayerCheckpoint(playerid);
								SetTimerEx("FarmerTimer", 5000, false, "i", playerid);
								break;
							}
						}
					}
				}
			}
			return 1;
		}
		else if(PTCAccepted[playerid] != 999)
		{
			PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
			PTCAccepted[playerid] = 999;
			GameTextForPlayer(playerid, "~w~Reached destination", 5000, 1);
	  		SafeSendClientMessage(playerid,COLOR_YELLOW, "당신은 목적지에 도착했습니다.");
			NomalCheckpoint[playerid] = false;
			DisablePlayerCheckpoint(playerid);
		}
		else if(Mechanic[playerid] != 999)
		{
			PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
			Mechanic[playerid] = 999;
			GameTextForPlayer(playerid, "~w~Reached destination", 5000, 1);
	  		SafeSendClientMessage(playerid,COLOR_YELLOW, "당신은 목적지에 도착했습니다.");
			NomalCheckpoint[playerid] = false;
			DisablePlayerCheckpoint(playerid);
		}
		else if(PlayerDrivingTest[playerid] != 0)
		{
			for(new cp = 0; cp < sizeof(DrivingTestPos); cp++)
			{
				if(IsPlayerInRangeOfPoint(playerid, 10.0, DrivingTestPos[cp][0], DrivingTestPos[cp][1], DrivingTestPos[cp][2]))
				{
					PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
					if(cp == sizeof(DrivingTestPos)-1)
					{
						PlayerInfo[playerid][pCarLic] = 1;
						DrivingTestEnd(playerid);
						SafeSendClientMessage(playerid,COLOR_YELLOW, "운전면허 시험에 통과하셨습니다.");
						DisablePlayerCheckpoint(playerid);
						return 1;
					}
					SetPlayerCheckpoint(playerid, DrivingTestPos[cp+1][0], DrivingTestPos[cp+1][1], DrivingTestPos[cp+1][2], 5.0);
					SafeSendClientMessage(playerid,COLOR_YELLOW, "다음 장소로 이동하십시오.");
					break;
				}
			}
			return 1;
		}
		else if(TruckStart[playerid] == true)
		{
			if(PlayerInfo[playerid][pJob] == 7)
			{
				if(IsPlayerInAnyVehicle(playerid) && GetVehicleModel(vehicleid) == 456)
				{
					for(new p = 0; p < sizeof(TruckPos); p++)
					{
						if(IsPlayerInRangeOfPoint(playerid, 5, TruckPos[p][0], TruckPos[p][1], TruckPos[p][2]))
						{
							if(p+1 == sizeof(TruckPos))
							{
							    new pay, cool;
								pay = GetVarInt("트럭임금");
								cool = GetVarInt("트럭쿨타임");
								NomalCheckpoint[playerid] = false;
								TruckStart[playerid] = false;
								SafeGivePlayerMoney(playerid, pay); // 원래 750
								DisablePlayerCheckpoint(playerid);
								if(HireCar[playerid] != 0) SetVehicleToRespawn(HireCar[playerid]);
								HireCar[playerid] = 0;
								PlayerInfo[playerid][pJobTime] = cool; // 원래 2800
								HireCarType[playerid] = 0;
								GameInfo[gProduct] += 60;
								new string[128];
								format(string, sizeof(string), "(정보) :"#C_WHITE" 트럭배달 일을 끝마쳤습니다. 총 수익은 $%d이고, 쿨타임은 %d입니다.", pay,cool);
								SafeSendClientMessage(playerid, COLOR_YELLOW, string);
						//		SafeSendClientMessage(playerid,COLOR_YELLOW, "석가 탄신일을 맞이하여 나라에서 150$ 더 주었습니다.");
								return 1;
							}
							SetPlayerCheckpoint(playerid, TruckPos[p+1][0], TruckPos[p+1][1], TruckPos[p+1][2], 5.0);
							return 1;
						}
					}
				}
			}
		}
	}
	else
	{
		PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
		DisablePlayerCheckpoint(playerid);
	}
	return 1;
}

