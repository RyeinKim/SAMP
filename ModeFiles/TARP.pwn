stock IsCanUsePhone(playerid)
{
    if(PlayerInfo[playerid][pJailed] != 0)
	{
		SafeSendClientMessage(playerid, COLOR_GRAY, "수감 중에는 사용할 수 없습니다.");
		return 0;
	}
	if(PlayerInfo[playerid][pPonline] == 1)
	{
		SafeSendClientMessage(playerid, COLOR_GRAY, "핸드폰 전원이 꺼져 있습니다.");
		return 0;
	}
	if(PlayerInfo[playerid][pPnumber] == 0)
	{
		SafeSendClientMessage(playerid, COLOR_GRAY, "핸드폰을 소지하고 있지 않습니다.");
		return 0;
	}
	if(SafeGetPlayerMoney(playerid) < 1)
	{
		SafeSendClientMessage(playerid, COLOR_GRAY, "당신은 통화비를 납부할 수 있는 소지금이 없습니다.");
		return 0;
	}
	return 1;
}

stock SendTaxiDriverMSG(COLOR,stext[])
{
	for(new i = 0; i < M_P; i++)
	{
		if(IsPlayerConnected(i))
		{
		    if(PlayerInfo[i][pJob] == 12 )
		    {
				SafeSendClientMessage(i, COLOR, stext);
			}
		}
	}
}

stock Call(playerid, phonenumb)
{
	new string[128], giveplayerid;
    format(string, sizeof(string), "* %s 이(가) 핸드폰을 꺼낸다.", PlayerNameEx(playerid));
    ApplyAnimation(playerid, "PED", "phone_in",FF[playerid],O1[playerid],O2[playerid],O3[playerid],O4[playerid],O5[playerid], 1);
	ProxDetector(5.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);

	if(phonenumb == 1313)
	{
		SafeSendClientMessage(playerid, COLOR_YELLOW, "State Bank: 안녕하세요, 산안드레스 주립은행 입니다.");
		SafeSendClientMessage(playerid, COLOR_YELLOW, "State Bank: 통장 잔액 확인은 \"1\"번을 눌러 주시기 바랍니다.");
		SafeSendClientMessage(playerid, COLOR_YELLOW, "State Bank: 통장 송금은 \"2\"번을 눌러 주시기 바랍니다.");
		Mobile[playerid] = 1313;
		return 1;
	}
	if(phonenumb == 7777)
	{
		format(string, sizeof(string), "* %s 이(가) 택시 호출을 요청합니다. (/동의 택시) ", PlayerNameEx(playerid));
		SendTaxiDriverMSG(COLOR_YELLOW, string);
		SafeSendClientMessage(playerid, COLOR_YELLOW, "TAXI: 택시를 이용해 주셔서 감사합니다. 콜택시 요청 수락까지 잠시만 기다려 주시기 바랍니다.");
		PTCCall = playerid;
		return 1;
	}
	if(phonenumb == 1655)
	{
		new mechanicworkers = 0;
		for(new i=0; i<M_P; i++)
		{
			if(IsPlayerConnected(i))
			{
				if(PlayerInfo[i][pJob] == 4)
				{
					mechanicworkers++;
				}
			}
		}
		if(mechanicworkers < 1)
		{
			SafeSendClientMessage(playerid, COLOR_GRAY, "업무중인 정비사가 없습니다.");
			return 1;
		}

		format(string, sizeof(string), "* %s 이(가) 정비사를 부릅니다. (/동의 하이카)", PlayerNameEx(playerid));
		SendJobMessage(4, COLOR_YELLOW, string);
		SafeSendClientMessage(playerid, COLOR_YELLOW, "Mechanic: 정비사가 출발하였습니다. 잠시만 기다려 주시기 바랍니다.");
		MechanicCall = playerid;
		return 1;
	}
	if(phonenumb == 633)
	{
		SafeSendClientMessage(playerid, COLOR_GRAD2, "이제 당신은 말을 할 수 있습니다.");
		SafeSendClientMessage(playerid, COLOR_YELLOW, "S.A.N network: 삐! 소리후 메시지를 남겨 주시기 바랍니다.");
		SafeSendClientMessage(playerid, COLOR_YELLOW, "삐!");
		Mobile[playerid] = 633;
		return 1;
	}
	if(phonenumb == 911)
	{
		SafeSendClientMessage(playerid, COLOR_YELLOW, "911 Call: 경찰 또는 의사 도움이 필요하십니까?");
		SafeSendClientMessage(playerid, COLOR_YELLOW, "911 Call: 경찰을 필요하면 '경찰'을, 의사가 필요하면 '의사'를 입력해 주시기 바랍니다.");
		Mobile[playerid] = 911;
		return 1;
	}
	if(phonenumb == PlayerInfo[playerid][pPnumber])
	{
		SafeSendClientMessage(playerid, COLOR_GRAD2, "전화 통화가 불가능한 전화번호입니다.");
		return 1;
	}
	if(Mobile[playerid] != 999)
	{
