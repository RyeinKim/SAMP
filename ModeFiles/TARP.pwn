forward public SaveVar(varid);
public SaveVar(varid)
{
	if (!VarLoading)
	{
		new file[256], str[256];
		file = "variables.txt";
		if (!fexist(file)) puni_Create(file);
		format(str, sizeof(str), "Name%d", varid);
		puni_Set(file, str, VarName[varid]);
		format(str, sizeof(str), "Value%d", varid);
		puni_Set(file, str, GetVarString(VarName[varid]));

	}
}
stock BackupVar()
{
	new file[256];
	if (!VarLoading)
	{
	    new year, month, day, hour, minute, second;
		getdate(year, month, day);
		gettime(hour, minute, second);
		format(file, sizeof file, "variables_Backup/variables_%d-%d-%d_%d-%d-%d.txt", year, month, day, hour, minute, second);
	    for (new varid; varid < MAX_VARS; varid++)
	    {
			new str[256];
			if (!fexist(file)) puni_Create(file);
			format(str, sizeof(str), "Name%d", varid);
			puni_Set(file, str, VarName[varid]);
			format(str, sizeof(str), "Value%d", varid);
			puni_Set(file, str, GetVarString(VarName[varid]));
		}
		return file;
	}
	return file;
}

forward public LoadVar();
public LoadVar()
{
	VarLoading = 1;
	new file[256], str[256];
	file = "variables.txt";
	if (!fexist(file))
	{
		VarLoading = 0;
		return 0;
	}
	for (new a = 0; a < MAX_VARS; a++)
	{
		new Name[256], Str[256];
	    format(str, sizeof(str), "Name%d", a);
	    strmid(Name, puni_Get(file, str), 0, 256, 256);
	    format(str, sizeof(str), "Value%d", a);
	    strmid(Str, puni_Get(file, str), 0, 256, 256);
	    printf("%d %s %s", a, Name, Str);
		if (strlen(Name))
		    SetVarString(Name, Str);
	}
	VarLoading = 0;
	fremove(file);
	for (new a = 0; a < MAX_VARS; a++)
	    SaveVar(a);
	return 1;
}

forward public DeleteVar(const Name[]);
public DeleteVar(const Name[])
{
	if (!strlen(Name)) return 0;
	for (new a = 0; a < MAX_VARS; a++)
	{
	    if (!strcmp(VarName[a], Name, true) && strlen(VarName[a]))
	    {
            DeleteGVar(Name);
            strmid(VarName[a], "", 0, 0, 0);
			SaveVar(a);
			return 1;
		}
	}
	return 0;
}

