public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
	new string[1024];
	new vehicleid = GetPlayerVehicleID(playerid);
	new giveplayerid;
	if(response)
		Audio_PlayStreamed(playerid, "http://pds24.egloos.com/pds/201204/13/28/button-3.mp3");
	else
		Audio_PlayStreamed(playerid, "http://pds24.egloos.com/pds/201204/13/28/button-10.mp3");
	if(gPlayerLogged[playerid])
	{
	    SaveUserLog(playerid, inputtext);
		//PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
	}
	if(dialogid==dialog_pass)
	{
		if(response)
		{
		    if(!strlen(inputtext))
		    {
		        return 1;
		    }
			if(strval(inputtext)==passwordgate)
			{
			    new objid;
			    if(IsPlayerInRangeOfPoint(playerid,2.0,247.599609375, 72.390556335449, 1004.200012207) || IsPlayerInRangeOfPoint(playerid,2.0,247.599609375, 72.390556335449, 1004.200012207))
