forward BloodHack();
public BloodHack()// 체력핵방지
{
	new i;
	new Float:health;
	for(i=0 ; i<MAX_PLAYERS ; i++)
	{
		if(!IsPlayerAdmin(i))
		{
			if(GetPlayerHealth(i, health) > 200)
			{
				Kick(i); // 킥
			}
		}
	}
	return 1;
}
forward RappelDown();
public RappelDown()
{
	for(new playerid=0;playerid<M_P;playerid++)
	{
	    if(IsPlayerConnected(playerid))
	    {
			if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && PKIN[playerid] == 0)
			{
			    new sweapon,sammo;
				for(new w = 0; w < 13; w++)
				{
					GetPlayerWeaponData(playerid, w, sweapon, sammo);
					if(PlayerInfo[playerid][pAGun][0]==sweapon&&sammo==0)
					{
					    PlayerInfo[playerid][pAGun][0]=0;
					    PlayerInfo[playerid][pGAmm][0]=0;
					    break;
					}
				}
				if(PlayerInfo[playerid][pGAmm][0]==0)
				{
					SetPlayerArmedWeapon(playerid,0);
				}else{
					SetPlayerArmedWeapon(playerid,PlayerInfo[playerid][pAGun][0]);
				}
			}
		  	if(RappelPlayerid[playerid]!=0)
		  	{
		  	    if(RappelVehicle[RappelPlayerid[playerid]]!=999)
		  	    {
		  	        LoopingAnim(playerid, "PED", "abseil", 4.0, 0, 1, 1, 1, -1);
		  	        new Float:tx,Float:ty,Float:tz,Float:x,Float:y,Float:z;
		  	        GetVehiclePos(RappelPlayerid[playerid],x,y,z);
		  	        //MapAndreas_FindZ_For2DCoord(x,y,z);
		  	        GetPlayerPos(playerid,tx,ty,tz);
		  	        SetPlayerVelocity(playerid,0,0,-0.2);
					z=floatadd(z,1.45);
		  	        if(z>=tz)
		  			{
		  	       		RappelPlayerid[playerid]=0;
		  	            ClearAnimations(playerid,1);
		  	        }
		  	    }else{
		  	        RappelPlayerid[playerid]=0;
		  	        ClearAnimations(playerid,1);
		  	    }
		  	}
  		}
  	}
}

forward Nitrodown();
public Nitrodown()
{
	for(new vehicleid=1;vehicleid<MAX_VEHICLES;vehicleid++)
	{
		if(Nitroused[vehicleid]==true&&NitroPower[vehicleid]>=1&&Nitrouse[vehicleid]==true)
		{
            NitroPower[vehicleid]--;
            if(NitroPower[vehicleid]<=0)
           	{
    			Nitrouse[vehicleid]=false;
                NitroPower[vehicleid]=0;
       		    RemoveVehicleComponent(vehicleid, 1010);
			}
		}
	}
}

forward Nitroup();
public Nitroup()
{
	for(new vehicleid=1;vehicleid<MAX_VEHICLES;vehicleid++)
	{
		if(Nitroused[vehicleid]==true&&NitroPower[vehicleid]<100&&Nitrouse[vehicleid]==false)
		{
            NitroPower[vehicleid]++;
            if(NitroPower[vehicleid]>=100)
           	{
                NitroPower[vehicleid]=100;
			}
		}
	}
}

forward Timer1000(playerid);
public Timer1000(playerid)
{
    if (LoadingTOS[playerid])
    {
        LoadingTOS[playerid] ++;
	    if (LoadingTOS[playerid] >= 5)
	    {
	        LoadingTOS[playerid] = 0;
	        KillTimer(IntroTimer[playerid]);
	        IntroTimer[playerid] = 0;
	        if(CheckAccount(GetName(playerid))) 
	        {
				new string[512];
				new ip[256];
				GetPlayerIp(playerid, ip, sizeof(ip));
			    format(string,sizeof(string),"%s%s님, The American ROLE PLAY GAMES 에 오신 것을 환영합니다.\n\nIP 주소: %s\n\n비밀번호를 입력하여 로그인해 주십시오.", C_WHITE, PlayerName(playerid), ip);
				ShowPlayerDialog(playerid, DIALOG_25, DIALOG_STYLE_PASSWORD, ""#C_BLUE"로그인",string, "로그인", "비번찾기");
			}
