public IsRentPlace(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		if(IsPlayerInRangeOfPoint(playerid, 2, 833.5123,-1819.7084,12.2545))
			return 1;
		else if(IsPlayerInRangeOfPoint(playerid, 2, 2140.2295,-1191.9250,23.9922))
			return 2;
