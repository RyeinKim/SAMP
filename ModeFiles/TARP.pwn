stock SaveAdminlog(string[])
{
	new filename[128];
	new entry[512];
	new File:hFile;
	format(filename, sizeof(filename), "Logs/Admin.log");
	new y, m, d;
	new h,mi,s;
	getdate(y,m,d);
	gettime(h,mi,s);
	format(entry, sizeof(entry), "(%d/%d/%d)[%d:%d:%d] %s\r\n",y,m,d,h,mi,s,string);
	hFile = fopen(filename, io_append);
	b_fwrite(hFile, entry);
	fclose(hFile);
}

