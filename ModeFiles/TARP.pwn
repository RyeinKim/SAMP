			//	SafeSendClientMessage(playerid,COLOR_WHITE, "정보를 불러오고 있습니다.");
				return 1;
			}
			if(PhoneNumbers < 10000)
			{
	    		PhoneNumbers = 10000;
	    		SavePhoneNumber(playerid);
				LoadPhoneNumber(playerid);
				return 1;
			}
			return 1;
		}
	}
	return 1;
}

