public LoadPrice(number)
{
 	new dir[256],resultprice;
    format(dir,sizeof(dir),"Price/%d.ini",number);
    if(!INI_Exist(dir))
    {
        return 0;
    }else{
    	INI_Open(dir,Read);
    	resultprice = strval(INI_Read("Price"));
		INI_Close();
