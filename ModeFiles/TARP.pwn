public IsAMember(type, playerid)
{
	if(IsPlayerConnected(playerid))
	{
		if(PlayerInfo[playerid][pMember] == 0)
		{
			return 0;
		}
		else if(FactionInfo[PlayerInfo[playerid][pMember]][fCreate] == 1)
		{
			if(type == 1)
			{
				if(FactionInfo[PlayerInfo[playerid][pMember]][fType] == 3 || FactionInfo[PlayerInfo[playerid][pMember]][fType] == 4)
					return 1;
			}
			else if(type == 2)
			{
				if(FactionInfo[PlayerInfo[playerid][pMember]][fType] == 4)
					return 1;
			}
		}
	}
	return 0;
}

