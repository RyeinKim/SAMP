public OnPlayerSpawn(playerid)
{
    if(gPlayerLogged[playerid] == false && !IsPlayerNPC(playerid))
	{
	    if (LoginScreen[playerid]) return 0;
	    LoginScreen[playerid] = true;
		new r = LoginScreens[playerid];
	    SafeSetPlayerPos(playerid, Intro[r][0], Intro[r][1], Intro[r][2]);
	    SetPlayerCameraPos(playerid, Intro[r][3], Intro[r][4], Intro[r][5]);
	    SetPlayerCameraLookAt(playerid, Intro[r][6], Intro[r][7], Intro[r][8]);
	    TogglePlayerControllable(playerid, 0);
	    ClearChatbox(playerid, 20);

		SPL[playerid] = 1;
        IntroTimer[playerid] = SetTimerEx("Timer1000", 1000, true, "i", playerid);
		TextDrawShowForPlayer(playerid, IntroTD[0]);
		TextDrawShowForPlayer(playerid, IntroTD[1]);
		TextDrawShowForPlayer(playerid, IntroTD[2]);
		TextDrawShowForPlayer(playerid, IntroTD[3]);
		return 1;
	}
	SetPlayerFightingStyle(playerid, FIGHT_STYLE_GRABKICK);
	RappelPlayerid[playerid]=0;
	IntroTD_Timer[playerid] = 0;
	SPL[playerid] = 0;
	TextDrawHideForPlayer(playerid, IntroTD[0]);
	TextDrawHideForPlayer(playerid, IntroTD[1]);
	TextDrawHideForPlayer(playerid, IntroTD[2]);
	TextDrawHideForPlayer(playerid, IntroTD[3]);
	TextDrawHideForPlayer(playerid, IntroTD[4]);
    AntiDeAMX();
	PlayerDeath[playerid] = false;
	if(PKIN[playerid] != 0)
	{
		SafeSetPlayerHealth(playerid, 100);
		if(PKIN[playerid] == 1)
		{
			SetPlayerVirtualWorld(playerid, 901);
			SetPlayerInterior(playerid, 0);
			new rand = random(2);
			if(rand == 1)
				SafeSetPlayerPos(playerid, 2139.2322,-2596.6013,13.5469);
			else
				SafeSetPlayerPos(playerid, -1341.5264,-408.0656,13.6330);
			for(new i = 0; i < M_P; i++)
			{
				if(IsPlayerConnected(i))
				{
					if(PKIN[playerid] == PKIN[i])
					{
						SetPlayerMarkerForPlayer(i, playerid, PKCOLOR[playerid]);
					}
				}
			}
		}
		else if(PKIN[playerid] == 2)
		{
			if(PKTEAM[playerid] == 0)
			{
	  			new BlueTeam = 0;
				new RedTeam = 0;
				for(new i = 0; i < M_P; i++)
				{
					if(IsPlayerConnected(i) && gPlayerLogged[i] == true)
			  		{
			  			if(PKIN[i] == PKIN[playerid])
			  			{
							if(PKTEAM[playerid] == 1)
							{
								BlueTeam ++;
							}
							else if(PKTEAM[playerid] == 2)
							{
								RedTeam ++;
							}
						}
					}
				}
				if(BlueTeam == RedTeam)
				{
					new TS = random(2);
					if(TS == 0)
					{
						SetPlayerTeam(playerid, 1);
						PKTEAM[playerid] = 1;
					}
					else if(TS == 1)
					{
						SetPlayerTeam(playerid, 2);
						PKTEAM[playerid] = 2;
					}
