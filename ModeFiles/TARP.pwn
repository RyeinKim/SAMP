	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		new vehicleid = GetPlayerVehicleID(playerid);
		if(IsPlayerInRangeOfPoint(playerid, 10.0, 2259.0073,2497.8491,1.4762))
		{
			new cp = GetVehicleVirtualWorld(vehicleid)-1;
