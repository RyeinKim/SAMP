public SaveBox(playerid, v)
{
	if(BoxInfo[playerid][v][bCreate])
	{
	    new dir[256];
	    format(dir,sizeof(dir),"Boxes/%s_%d.ini",GetName(playerid),v);
	    INI_Open(dir,Write);
	    new string[1024];
		INI_WriteInt("B_Create",BoxInfo[playerid][v][bCreate]);
		INI_WriteInt("B_Lock",BoxInfo[playerid][v][bLock]);
		for(new a; a < 3; a++)
		{
		    format(string, sizeof(string), "B_Pos_%d'",a);
		    INI_WriteFloat(string,BoxInfo[playerid][v][bPos][a]);
		}
		INI_WriteInt("B_Vir",BoxInfo[playerid][v][bVir]);
 		INI_WriteInt("B_Int",BoxInfo[playerid][v][bInt]);
		for(new a; a < MAX_BOXSLOTS; a++)
		{
			format(string, sizeof(string), "B_Thing_%d", a, BoxInfo[playerid][v][bThing][a]);
			INI_WriteInt(string,BoxInfo[playerid][v][bThing][a]);
			format(string, sizeof(string), "B_Amount_%d", a, BoxInfo[playerid][v][bAmount][a]);
			INI_WriteInt(string,BoxInfo[playerid][v][bAmount][a]);
			format(string, sizeof(string), "B_Ammo_%d", a, BoxInfo[playerid][v][bAmmo][a]);
			INI_WriteInt(string,BoxInfo[playerid][v][bAmmo][a]);
		}
		INI_Close();
	}
	return 1;
}

public LoadBox(playerid, v)
{
	if(!BoxInfo[playerid][v][bCreate])
	{
		if (CheckBox(playerid, v))
		{
		    new dir[256];
		    format(dir,sizeof(dir),"Boxes/%s_%d.ini",GetName(playerid),v);
			if(INI_Exist(dir))
			{
			    INI_Open(dir,Read);
			    new string[1024];
				BoxInfo[playerid][v][bCreate]=strval(INI_Read("B_Create"));
				BoxInfo[playerid][v][bLock]=strval(INI_Read("B_Lock"));
				for(new a; a < 3; a++)
				{
				    format(string, sizeof(string), "B_Pos_%d'",a);
				    BoxInfo[playerid][v][bPos][a]=floatstr(INI_Read(string));
				}
				INI_WriteInt("B_Vir",BoxInfo[playerid][v][bVir]);
		 		INI_WriteInt("B_Int",BoxInfo[playerid][v][bInt]);
				for(new a; a < MAX_BOXSLOTS; a++)
				{
					format(string, sizeof(string), "B_Thing_%d", a, BoxInfo[playerid][v][bThing][a]);
					BoxInfo[playerid][v][bThing][a]=strval(INI_Read(string));
					format(string, sizeof(string), "B_Amount_%d", a, BoxInfo[playerid][v][bAmount][a]);
					BoxInfo[playerid][v][bAmount][a]=strval(INI_Read(string));
					format(string, sizeof(string), "B_Ammo_%d", a, BoxInfo[playerid][v][bAmmo][a]);
					BoxInfo[playerid][v][bAmmo][a]=strval(INI_Read(string));
				}
				INI_Close();
			}
		}
	}
	return 1;
}

public CreateBox(playerid)
{
    for(new a = 0; a < MAX_PBOXES; a++)
    {
        if(!BoxInfo[playerid][a][bCreate])
        {
            new Float:x, Float:y, Float:z;
			GetPlayerPos(playerid, x, y, z);
			BoxInfo[playerid][a][bPos][0] = x;
			BoxInfo[playerid][a][bPos][1] = y;
			BoxInfo[playerid][a][bPos][2] = z;
			BoxInfo[playerid][a][bVir] = GetPlayerVirtualWorld(playerid);
			BoxInfo[playerid][a][bInt] = GetPlayerInterior(playerid);
			BoxInfo[playerid][a][bLock] = 0;
			for(new b; b < MAX_BOXSLOTS; b++)
			{
			    BoxInfo[playerid][a][bThing][b] = BoxInfo[playerid][a][bAmount][b] = BoxInfo[playerid][a][bAmmo][b] = 0;
			}
			BoxInfo[playerid][a][bObj] = CreateDynamicObject(BOX_OBJMODEL, BoxInfo[playerid][a][bPos][0], BoxInfo[playerid][a][bPos][1], BoxInfo[playerid][a][bPos][2]-0.6, 0.0, 0.0, 0.0, BoxInfo[playerid][a][bVir], BoxInfo[playerid][a][bInt], -1, 500);
			BoxInfo[playerid][a][bCreate] = 1;
			return a;
		}
	}
	return INVALID_BOX_ID;
}

public DestroyBox(playerid, boxid)
{
	if(BoxInfo[playerid][boxid][bCreate])
	{
		BoxInfo[playerid][boxid][bCreate] = 0;
		DestroyDynamicObject(BoxInfo[playerid][boxid][bObj]);
		for(new a; a < 3; a++) BoxInfo[playerid][boxid][bPos][a] = 0;
		BoxInfo[playerid][boxid][bVir] = BoxInfo[playerid][boxid][bInt] = 0;
		for(new b; b < MAX_BOXSLOTS; b++)
		    BoxInfo[playerid][boxid][bThing][b] = BoxInfo[playerid][boxid][bAmount][b] = BoxInfo[playerid][boxid][bAmmo][b] = 0;
	}
	return 1;
}

