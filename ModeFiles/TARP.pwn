//		printf(string);
		for(new i = 0; i < M_P; i++)
		{
			if(IsPlayerConnected(i))
			{
				if(PlayerInfo[i][pRealadmin] >= 1 && CheckHackMessage[i] == 0)
				{
					SafeSendClientMessage(i, 0xFFFFFFAA, string);
				}
			}
		}
	}

	if(ClothesSellect[playerid] != 0)
	{
		new keys[3];
		GetPlayerKeys(playerid, keys[0], keys[1], keys[2]);
		if(keys[2] == KEY_RIGHT)
		{
			ClothesSellect[playerid] ++;
			if(PlayerInfo[playerid][pSex] == 1)
			{
				if(ClothesSellect[playerid] == sizeof(CivMalePeds)+1)
				{
					ClothesSellect[playerid] = 1;
				}
				SetPlayerSkin(playerid, CivMalePeds[ClothesSellect[playerid]-1]);
			}
			if(PlayerInfo[playerid][pSex] == 2)
			{
				if(ClothesSellect[playerid] == sizeof(CivFemalePeds)+1)
				{
					ClothesSellect[playerid] = 1;
				}
				SetPlayerSkin(playerid, CivFemalePeds[ClothesSellect[playerid]-1]);
			}
   			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		}
		else if(keys[2] == KEY_LEFT)
		{
			ClothesSellect[playerid] --;
			if(PlayerInfo[playerid][pSex] == 1)
			{
				if(ClothesSellect[playerid] == 0)
				{
					ClothesSellect[playerid] = sizeof(CivMalePeds);
				}
				SetPlayerSkin(playerid, CivMalePeds[ClothesSellect[playerid]-1]);
			}
			if(PlayerInfo[playerid][pSex] == 2)
			{
				if(ClothesSellect[playerid] == 0)
				{
					ClothesSellect[playerid] = sizeof(CivFemalePeds);
				}
				SetPlayerSkin(playerid, CivFemalePeds[ClothesSellect[playerid]-1]);
			}
   			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		}
	}
	if(g_fSpeedCap[playerid] != 0.0 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && !BikeJump[playerid])
	{
		new Float:s_fX,Float:s_fY,Float:s_fZ,Float:s_fVX,Float:s_fVY,Float:s_fVZ;
		GetVehiclePos(vehicleid, s_fX, s_fY, s_fZ );
		GetVehicleVelocity(vehicleid, s_fVX, s_fVY, s_fVZ );
		if( !IsPlayerInRangeOfPoint( playerid, g_fSpeedCap[playerid] + 0.05, s_fX + s_fVX, s_fY + s_fVY, s_fZ + s_fVZ ) )
		{
			new Float:s_fLength;
			s_fLength = floatsqroot( ( s_fVX * s_fVX ) + ( s_fVY * s_fVY ) + ( s_fVZ * s_fVZ ) );
			s_fVX = ( s_fVX / s_fLength ) * g_fSpeedCap[playerid];
			s_fVY = ( s_fVY / s_fLength ) * g_fSpeedCap[playerid];
			s_fVZ = ( s_fVZ / s_fLength ) * g_fSpeedCap[playerid];
			SetVehicleVelocity( vehicleid, s_fVX, s_fVY, s_fVZ );
		}
	}
	return 1;
}

stock IsPlayerAiming(playerid, aimid)
{
	new Float:X1, Float:Y1, Float:Z1, Float:X2, Float:Y2, Float:Z2;
	GetPlayerPos(playerid, X1, Y1, Z1);
	GetPlayerPos(aimid, X2, Y2, Z2);
	new Float:Distance = floatsqroot(floatpower(floatabs(X1-X2), 2) + floatpower(floatabs(Y1-Y2), 2));
	if(Distance < 35)
	{
		new Float:A;
		GetPlayerFacingAngle(playerid, A);
		X1 += (Distance * floatsin(-A, degrees));
		Y1 += (Distance * floatcos(-A, degrees));
		Distance = floatsqroot(floatpower(floatabs(X1-X2), 2) + floatpower(floatabs(Y1-Y2), 2));
		if(Distance < 1.0)
		{
			return true;
		}
	}
	return false;
}

