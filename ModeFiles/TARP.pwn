public ConnectScreen(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		if(gPlayerLogged[playerid] == false)
		{
			if(LoginCameraTime[playerid] > 5000)
			{
				LoginCameraPos[playerid][0] -= 2 * floatsin(LoginCameraPos[playerid][2], degrees) + 0.5;
				LoginCameraPos[playerid][1] += 0.5 * floatsin(LoginCameraPos[playerid][2], degrees) + 0.5;
			}
			else
			{
				LoginCameraPos[playerid][0] += 2 * floatsin(-LoginCameraPos[playerid][2], degrees) + 0.5;
				LoginCameraPos[playerid][1] -= 0.5 * floatsin(-LoginCameraPos[playerid][2], degrees) + 0.5;
			}
			SetPlayerCameraPos(playerid, LoginCameraPos[playerid][0], LoginCameraPos[playerid][1], 80);
			SetPlayerCameraLookAt(playerid, 0, 0, 70);
			SafeSetPlayerPos(playerid, LoginCameraPos[playerid][0], LoginCameraPos[playerid][1], -50);
			LoginCameraPos[playerid][2] += 0.1;
			LoginCameraTime[playerid] ++;
			if(LoginCameraTime[playerid] > 16000)
			{
				LoginCameraPos[playerid][0] = 0;
				LoginCameraPos[playerid][1] = 0;
				LoginCameraTime[playerid] = 0;
			}
			SetTimerEx("ConnectScreen", 7, false, "i", playerid);
		}
		else
		{
			SpawnPlayer(playerid);
		}
	}
	return 1;
}

