		    SetVehicleParamsEx(ve,engine,lights,alarm,1,bonnet,boot,objective);
		}
		if(VehicleInfo[ve][vTaxiCab]==1)
		{
		    VehicleTaxiCab[ve]=CreateDynamicObject(19308,0,0,0,0,0,0);
			AttachDynamicObjectToVehicle(VehicleTaxiCab[ve], ve, -0.000000,-0.494999,0.92000,0.000000,0.000000,0.000000);
