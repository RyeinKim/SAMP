public OnVehicleUpdate(vehicleid)
{
	if(GetVehicleModel(vehicleid)==0)
	{
	    KillTimer(UpdateVehicleTimer[vehicleid]);
	    return 1;
	}
	new Float:vhealth,string[256];
	GetVehicleHealth(vehicleid, vhealth);
	new engine,lights,alarm,doors,bonnet,boot,objective;
	GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
	if(VehicleInfo[vehicleid][vLock] == 1 && doors != 1)
	{
		SetVehicleParamsEx(vehicleid,engine,lights,alarm,1,bonnet,boot,objective);
	}
	else if(VehicleInfo[vehicleid][vLock] == 0 && doors == 1)
	{
		SetVehicleParamsEx(vehicleid,engine,lights,alarm,0,bonnet,boot,objective);
	}
	if(VehicleInfo[vehicleid][vHP]==4001)
	{
		SetVehicleHealth(vehicleid, VehicleInfo[vehicleid][vPHP]);
		VehicleInfo[vehicleid][vHP]=VehicleInfo[vehicleid][vPHP];
	}else{
		if(VehicleInfo[vehicleid][vHP]<vhealth)
		{
			if(VehicleInfo[vehicleid][vHP]<=300)
			{
			    VehicleInfo[vehicleid][vHP]=300;
			}
			SetVehicleHealth(vehicleid, VehicleInfo[vehicleid][vHP]);
		}else{
			GetVehicleHealth(vehicleid, VehicleInfo[vehicleid][vHP]);
		}
		if(VehicleInfo[vehicleid][vPHP]<vhealth)
		{
			SetVehicleHealth(vehicleid, VehicleInfo[vehicleid][vPHP]);
		}
	}
	if(vhealth <= 300)
	{
	    if(RentInfo[vehicleid][rRentTime]!=0)
	    {
		    if(RentInfo[vehicleid][rSpawnType]==1)
		    {
			    if(IsPlayerConnected(RentInfo[vehicleid][rRentUser]))
			    {
			        SCM(RentInfo[vehicleid][rRentUser],COLOR_WHITE,"렌트카가 파손되어 대여가 끝났습니다");
			    }
				RentCar[RentInfo[vehicleid][rRentUser]]=0;
				RentInfo[vehicleid][rRentUser]=9999;
				RentInfo[vehicleid][rRentTime]=0;
			    SerVehicleHP(vehicleid,1000);
			    SetVehicleToRespawn(vehicleid);
		    }else if(RentInfo[vehicleid][rSpawnType]==2){
				OnDestroyVehicle(vehicleid);
				ResetVehicle(vehicleid);
			}
	    }else{
			SetVehicleParamsEx(vehicleid,0,0,alarm,doors,bonnet,boot,objective);
			SetVehicleHealth(vehicleid, 300);
			if(VehicleInfo[vehicleid][vCheckRepair]==0 && VehicleInfo[vehicleid][vFaction]==0 && VehicleInfo[vehicleid][vSign]!=0)
			{
			    VehicleInfo[vehicleid][vCheckRepair]=1;
			    for(new playerid=0;playerid<M_P;playerid++)
			    {
			        if(IsPlayerConnected(playerid))
			        {
			            if(IsPlayerInVehicle(playerid,vehicleid))
			            {
							if(IsCarOwner(playerid,vehicleid))
							{
								VehicleInfo[vehicleid][vPHP]=floatsub(VehicleInfo[vehicleid][vPHP],float(random(11)+20));
								SCM(playerid, COLOR_LIGHTRED, "차량의 내구도가 감소하였습니다.");
								if(VehicleInfo[vehicleid][vPHP]<349)
								{
									format(string, sizeof(string), "Vehicle/%d.ini", VehicleInfo[vehicleid][vSign]);
									INI_Remove(string);
									PlayerVehicleModel[playerid][CarSlot(playerid,vehicleid)]=0;
								    PlayerVehicleSign[playerid][CarSlot(playerid,vehicleid)]=0;
									PlayerVehicleTowed[playerid][CarSlot(playerid,vehicleid)]=0;
									OnDestroyVehicle(vehicleid);
									ResetVehicle(vehicleid);
									ResetCarOwner(playerid,vehicleid);
									PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
									SCM(playerid, COLOR_LIGHTRED, "차량을 더 이상 사용할수가 없어 폐차되었습니다.");
								}
								break;
							}
						}
					}
				}
			}
		}
	}
	else if(vhealth >= 700)
	{
		new panels, doors2, lights2, tires;
		GetVehicleDamageStatus(vehicleid, panels, doors2, lights2, tires);
		UpdateVehicleDamageStatus(vehicleid, 0, 0, 0, tires);
	}
	if(!IsPlayerConnected(RappelVehicle[vehicleid]))
	{
	    RappelVehicle[vehicleid]=999;
	    DestroyObject(RappelObject[vehicleid]);
	}
 	if(RappelVehicle[vehicleid]!=999)
 	{
	 	SetVehicleVelocity(vehicleid,0,0,0);
 	}
	new item = GetVehicleComponentInSlot(vehicleid, 5);
	if(item == 1010 && Nitroused[vehicleid]==false)
	{
		Nitroused[vehicleid]=true;
		NitroPower[vehicleid]=100;
		RemoveVehicleComponent(vehicleid,1010);
	}else if(item == 1009 && Nitroused[vehicleid]==true)
	{
		Nitroused[vehicleid]=false;
		NitroPower[vehicleid]=0;
	}else if(item == 1008 && Nitroused[vehicleid]==true)
	{
		Nitroused[vehicleid]=false;
		NitroPower[vehicleid]=0;
	}
	return 1;
}

