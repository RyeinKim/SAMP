public PlayerTimer500(playerid)
{
	new Float:health=0.0,Float:armour=0.0;
	GetPlayerHealth(playerid,health);
	GetPlayerArmour(playerid,armour);
	if(PlayerInfo[playerid][pDeath]!=0)
	{
		SetPHealth(playerid, 50);
	}
	if(HackHealth[playerid] == 0.0)
	{
	    if(togglestatus[playerid]==0)
	    {
			SetPlayerHealth(playerid,1000.0);
		}else{
			SetPHealth(playerid, 50);
		}
	}
	else if(health > HackHealth[playerid] && GetPlayerState(playerid) != PLAYER_STATE_NONE && GetPlayerState(playerid) != PLAYER_STATE_NONE && GetPlayerState(playerid) != PLAYER_STATE_SPAWNED && rugmod[playerid]==false && PlayerInfo[playerid][pUseAdmin]==0)
	{
	    if(togglestatus[playerid]==0)
	    {
			SetPlayerHealth(playerid,1000.0);
		}else if(FoodEating[playerid]==0)
	    {
			SetPHealth(playerid, HackHealth[playerid]);
		}
	}
	else if(health < HackHealth[playerid] && rugmod[playerid]==false && PlayerInfo[playerid][pUseAdmin]==0)
	{
	    if(health!=0)
	    {
			HackHealth[playerid] = health;
		}
	}
	else if(health > 100 && rugmod[playerid]==false && PlayerInfo[playerid][pUseAdmin]==0)
	{
	    if(togglestatus[playerid]==0)
	    {
			SetPlayerHealth(playerid,1000.0);
		}else{
			SetPHealth(playerid,100);
		}
	}
	if(PlayerInfo[playerid][pUseAdmin]!=0)
	{
	    SetPlayerHealth(playerid,99999);
	}
	if(armour > HackArmour[playerid] && GetPlayerState(playerid) != PLAYER_STATE_NONE && GetPlayerState(playerid) != PLAYER_STATE_NONE && GetPlayerState(playerid) != PLAYER_STATE_SPAWNED && PlayerInfo[playerid][pUseAdmin]==0)
	{
		SetPArmour(playerid, HackArmour[playerid]);
	}
	else if(armour < HackArmour[playerid] && PlayerInfo[playerid][pUseAdmin]==0)
	{
	    if(armour!=0)
	    {
			HackArmour[playerid] = armour;
		}
	}
	else if(armour > 200 && PlayerInfo[playerid][pUseAdmin]==0)
	{
		SetPArmour(playerid,200);
	}else if(PlayerInfo[playerid][pUseArmour]==0)
	{
	    SetPlayerArmour(playerid,0);
	}
	if(GetPlayerMoney(playerid) != GetMoney(playerid))
	{
		if(TunningPlayer[playerid]==0)
		{
			ResetPlayerMoney(playerid);
		    GivePlayerMoney(playerid,GetMoney(playerid));
		}
	}
	format(string_3D1[playerid], 256, "%s(%d)\n\
	{FFFFFF}Health: {FDE39D}%.1f\n\
	{FFFFFF}Armour: {FDE39D}%.1f\n\
	{FFFFFF}Ping: {FDE39D}%d\n\
	{FFFFFF}FPS: {FDE39D}%d", PlayerName(playerid),playerid,health, armour,GetPlayerPing(playerid),GetPlayerFPS(playerid));
	for(new i=0;i<M_P;i++)
	{
		if(IsPlayerConnected(i) && UseText[i][playerid]==1 && i!=playerid)
		{
		    if(PlayerInfo[i][pUseAdmin]==0)
		    {
				UpdatePlayer3DTextLabelText(i,ShowInfo[i][playerid],0xFFFFFF,"");
		    }else{
				UpdatePlayer3DTextLabelText(i,ShowInfo[i][playerid],0xFFFFFF,string_3D1[playerid]);
		    }
		}
	}
	GetPlayerPos(playerid,PlayerInfo[playerid][pPPos][0],PlayerInfo[playerid][pPPos][1],PlayerInfo[playerid][pPPos][2]);
	GetPlayerFacingAngle(playerid,PlayerInfo[playerid][pPPos][3]);
	PlayerInfo[playerid][pPVW]=GetPlayerVirtualWorld(playerid);
	PlayerInfo[playerid][pPIT]=GetPlayerInterior(playerid);
	return 1;
}

