public OnRconLoginAttempt(ip[], password[], success)
{
    if(success)
    {
        new IP[32],string[256];
		new year,month,day,hour,minute,second;
		getdate(year, month, day);
		gettime(hour,minute,second);
        for(new playerid=0;playerid<M_P;playerid++)
        {
            GetPlayerIp(playerid,IP,32);
            if(!strcmp(ip,IP,true))
            {
				if(PlayerInfo[playerid][pAdmin]==0)
				{
					format(string, sizeof(string), "%s님이 서버 접근을 시도하여 킥 되었습니다.이유: %s (%d-%d-%d)", PlayerName(playerid), "Auto",year, month,day);
					SendAdminMessage(COLOR_WARNING,string,1);
					KickEx(playerid);
				    return 1;
				}
				break;
            }
        }
        return 1;
    }
	return 1;
}

