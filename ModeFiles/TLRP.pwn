public OnPlayerDisconnect(playerid, reason)
{
	new string[256],Reason[32];
	if(PlayerInfo[playerid][pUseAdmin]==0)
	{
	    PlayerInfo[playerid][pHealth]=HackHealth[playerid];
	    if(PlayerInfo[playerid][pUseArmour]==1)
	    {
	    	PlayerInfo[playerid][pArmour]=HackArmour[playerid];
		}
	}
	switch(reason)
	{
		case 0:	Reason = "타임아웃",PlayerInfo[playerid][pCrash] = 1;
		case 1: Reason = "정상종료",PlayerInfo[playerid][pCrash] = 0;
		case 2: Reason = "강퇴",PlayerInfo[playerid][pCrash] = 1;
	}
	if(PlayerInfo[playerid][pUseAdmin]!=0)
	{
		format(string, sizeof(string), "관리자 %s 님이 %s 종료 되셨습니다.", PlayerInfo[playerid][pAdminName], Reason);
		ProxDetector(15.0, playerid, string, COLOR_YELLOW, COLOR_YELLOW, COLOR_YELLOW, COLOR_YELLOW, COLOR_YELLOW);
	}
	else
	{
		format(string, sizeof(string), "%s 님이 %s 종료 되셨습니다.", PlayerName(playerid), Reason);
		ProxDetector(15.0, playerid, string, COLOR_YELLOW, COLOR_YELLOW, COLOR_YELLOW, COLOR_YELLOW, COLOR_YELLOW);
	}
   	format(string ,sizeof(string), "%s 님이 접속종료 하였습니다. (%d, %s)",PlayerName(playerid),playerid,PlayerInfo[playerid][pIP]);
	SendAdminMessage(0xFFFF00AA, string);
	if(shutdown==1)
	{
	    PlayerInfo[playerid][pCrash] = 1;
	}
	if(PlayerInfo[playerid][pJailed]!=0)
	{
	    PlayerInfo[playerid][pCrash] = 0;
	}
	if(PlayerInfo[playerid][pPhoneStatus]!=9999 && PlayerInfo[playerid][pPhoneStatus]!=9998)
	{
	    if(PlayerInfo[playerid][pPhoneStatus]<1000)
	    {
	        RemovePlayerAttachedObject(PlayerInfo[playerid][pPhoneStatus], FIRE_OBJECT_SLOT);
			SCM(PlayerInfo[playerid][pPhoneStatus],COLOR_WHITE,"상대방이 전화를 끊었습니다.");
			KillTimer(KillTimer7[PlayerInfo[playerid][pPhoneStatus]]);
			PlayerInfo[PlayerInfo[playerid][pPhoneStatus]][pPhoneStatus]=9998;
	    }else if(PlayerInfo[playerid][pPhoneStatus]<2000){
			SCM(PlayerInfo[playerid][pPhoneStatus]-1000,COLOR_WHITE,"상대방이 전화를 끊었습니다.");
			PlayerInfo[PlayerInfo[playerid][pPhoneStatus]-1000][pPhoneStatus]=9998;
            PhoneRing[PlayerInfo[playerid][pPhoneStatus]-1000]=0;
		}else{
			PlayerInfo[playerid][pPhoneStatus]=9998;
		}
	    KillTimer(KillTimer7[playerid]);
	}
    if(TalkingLive[playerid] != 9999)
    {
        SCM(TalkingLive[playerid], COLOR_WHITE, "상대방과의 회담을 종료했습니다.");
        TalkingLive[TalkingLive[playerid]] = 9999;
        TalkingLive[playerid] = 9999;
    }
    if(TracePlayer[playerid]!=0)
    {
		for(new i=0;i<M_P;i++)
		{
		    if(IsPlayerConnected(i))
			{
			    if(TraceUsed[i]==playerid)
			    {
			        TraceUsed[i]=9999;
			        SCM(i,COLOR_WHITE,"추적 대상이 종료했습니다.");
			    }
			}
		}
	}
	for(new i=0;i<M_P;i++)
	{
	    if(IsPlayerConnected(i))
		{
