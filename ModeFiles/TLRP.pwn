    OnPlayerSave(playerid);
 	for(new i=0;i<M_P;i++)
 	{
 	    if(IsPlayerConnected(i))
 	    {
 	        if(PlayerSpec[i]==playerid && PlayerSpec[i]!=9999)
 	        {
				SCM(i,COLOR_WHITE,"위성을 보던 대상이 종료했습니다.");
