		for(new ce=0;ce<17;ce++)
		{
			format(string,sizeof(string),"vTune%d",ce);VehicleInfo[ve][vTune][ce]=strval(INI_Read(string));
		}
		for(new tr = 0; tr < GMT(VehicleInfo[ve][vModel]); tr++)
		{
			format(string,sizeof(string),"vTrunkType%d",tr);VehicleInfo[ve][vTrunkType][tr]=strval(INI_Read(string));
			format(string,sizeof(string),"vTrunk_%d",tr);VehicleInfo[ve][vTrunk][tr]=strval(INI_Read(string));
			format(string,sizeof(string),"vTrunk2_%d",tr);VehicleInfo[ve][vTrunk2][tr]=strval(INI_Read(string));
			format(string,sizeof(string),"vTrunk3_%d",tr);VehicleInfo[ve][vTrunk3][tr]=strval(INI_Read(string));
		}
		VehicleInfo[ve][vTrunkLock]=strval(INI_Read("vTrunkLock"));
