		VehicleOnFireObject[vehicleid]=CreateDynamicObject(18690, 0, 0, 0, 0, 0, 0);
		AttachDynamicObjectToVehicle(VehicleOnFireObject[vehicleid], vehicleid, 0.0, 0.4, -0.5, 0, 0, 0);
		VehicleOnFire[vehicleid] = 1;
		KillTimer(VehicleOnFireTimer[vehicleid]);
		KillTimer(VehicleOnFireTimer2[vehicleid]);
		VehicleOnFireTimer[vehicleid] = SetTimerEx("BurningVehicleTimer", 100, 1, "d", vehicleid);
