public BuyVehicle(playerid,modelid,price)
{
	new hc=0,string[256];
	for(new pcr=0;pcr<MAX_HVCAR;pcr++)
	{
	    if(PlayerVehicleSign[playerid][pcr]==0)
	    {
	        hc=pcr;
	        break;
	    }
	    if(pcr==MAX_HVCAR-1)
	    {
		    if(PlayerVehicleSign[playerid][pcr]!=0)
		    {
			    SCM(playerid,COLOR_WHITE,"당신은 더 이상 이것을 구매할 수 없습니다.");
		        return 1;
		    }
	    }
	}
	if(PlayerInfo[playerid][pBank]<price)
	{
	    SCM(playerid,COLOR_WHITE,"은행잔고에 잔금이 없습니다.");
	    return 1;
	}
	PlayerInfo[playerid][pBank]-=price;
	new Float:gpx,Float:gpy,Float:gpz,gvw;
