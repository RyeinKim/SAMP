		RentCar[playerid]=0;
	}
	CheckPoint[playerid]=0;
	DisablePlayerRaceCheckpoint(playerid);
	return 1;
}

public OnPlayerCommandText(playerid, cmdtext[])
{
	new cmd[256],tmp[256],idx,string[512],giveplayerid,vehicleid,givevehicleid;
	new num;
	new year, month,day,hour,minute,second;
	cmd = strtok(cmdtext,idx);

	getdate(year, month, day);
	gettime(hour,minute,second);
	vehicleid=GetPlayerVehicleID(playerid);
	givevehicleid=GetPlayerVehicleID(giveplayerid);

	if(PlayerLogin[playerid]==false && PlayerInfo[playerid][pReg]!=100)
	{
	    return 1;
	}
	for(new c=0;c<MAX_COMMAND;c++)
	{
		if(CommandDelay[playerid][c]!=0)
		{
			if(strfind(cmdtext,StopCommand[playerid][c],true)==0)
			{
			    SCM(playerid,COLOR_WHITE,"잠시후에 다시 시도하세요.");
				printf("COMMAND %s : (쿨타임 %d초남음) %s",GetName(playerid),CommandDelay[playerid][c],cmdtext);
			    return 1;
			}
		}
	}
	for(new i=0;i<M_P;i++)
 	{
 	    if(IsPlayerConnected(i))
 	    {
 	        if(PlayerSpec[i]==playerid && PlayerSpec[i]!=9999)
 	        {
 	            format(string,sizeof(string),"COMMAND %s(%d): %s",GetName(playerid),playerid,cmdtext);
				SCM(i,COLOR_WHITE,string);
			}
		}
 	}
	printf("COMMAND %s(%d): %s",GetName(playerid),playerid,cmdtext);

	if(strcmp(cmd, "/죽음", true) == 0)
	{
		if(IsPlayerConnected(playerid))
		{
		    if(PlayerInfo[playerid][pDeath] == 2)
		    {
				format(string, sizeof(string), "당신은 죽음을 선택하였으며, 시간이 지나면 리스폰됩니다. [남은 시간 %d 초]",PlayerInfo[playerid][pDeathTime]);
				SCM(playerid,COLOR_WHITE, string);
		        return 1;
		    }
			if( ( PlayerInfo[playerid][pDeath] != 1 && PlayerInfo[playerid][pDeath] != 3 ) || (PlayerInfo[playerid][pDeathTime] != 0))
			{
				format(string, sizeof(string), "죽음을 선택할 수 없습니다.[남은 시간 %d 초]",PlayerInfo[playerid][pDeathTime]);
				SCM(playerid,COLOR_WHITE, string);
				return 1;
			}
			togoocpm[playerid]=1;
            SetPlayerColor(playerid, COLOR_SPEC);
			PlayerInfo[playerid][pDeath] = 2;
			PlayerInfo[playerid][pDeathTime] = 400+random(60);
			format(string, sizeof(string), "당신은 죽음을 선택하였으며, 시간이 지나면 리스폰됩니다. [남은 시간 %d 초]",PlayerInfo[playerid][pDeathTime]);
			SCM(playerid,COLOR_WHITE, string);
		}
		return 1;
	}

	if(strcmp(cmd, "/wake", true) == 0)
	{
		if(IsPlayerConnected(playerid))
		{
		    if(PlayerInfo[playerid][pDeath]==0)
		    {
				SCM(playerid,COLOR_WHITE, "당신은 쓰러져있지 않습니다.");
		        return 1;
		    }
		    if(PlayerInfo[playerid][pDeath] != 1 || PlayerInfo[playerid][pDeathReason] != 2)
		    {
				SCM(playerid,COLOR_WHITE, "당신은 일어나기가 힘들정도로 고통을 느낍니다.");
		        return 1;
		    }
		    if(PlayerInfo[playerid][pDeath] == 1 && PlayerInfo[playerid][pDeathReason] == 2 && PlayerInfo[playerid][pDeathTime]!=0)
		    {
				format(string, sizeof(string), "일어나기까지 아직은 멀었습니다. [남은 시간 %d 초]",PlayerInfo[playerid][pDeathTime]);
				SCM(playerid,COLOR_WHITE, string);
		        return 1;
		    }
		    switch(random(1000))
		    {
		        case 0..400:
				{
					format(string, sizeof(string), " %s이(가) 천천히 바닥에서 일어난다.", PlayerName(playerid));
					BProxDetector(15.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					SCM(playerid,COLOR_WHITE, "당신은 정신이 들었습니다.");
					PlayerInfo[playerid][pDeath]=0;
					PlayerInfo[playerid][pDeathReason]=0;
					PlayerInfo[playerid][pDeathTime]=0;
					Anim(playerid,"BEACH", "bather",4.1,0,1,1,1,1);
					TogglePlayerControllable(playerid, 1);
				}
				case 401..999:
				{
				    PlayerInfo[playerid][pDeathReason]=4;
					SCM(playerid,COLOR_WHITE, "당신은 일어나기가 힘들정도로 고통을 느낍니다.");
				}
		    }
		}
		return 1;
	}

	if(strcmp(cmd, "/관리자",true) == 0)
	{
		if(IsPlayerConnected(playerid))
		{
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[256];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result))
			{
				SCM(playerid,COLOR_WHITE, "/관리자 [내용]");
