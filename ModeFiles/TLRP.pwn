CheckPlayerName(const name[])
{
	if (strlen(name) < 5) return 2;
	if (strfind(name, "_", true) == -1) return 3;
	new underscorecount, expectinguppercase = 1;
	for (new i = 0, j = strlen(name); i < j; i++)
	{
		if (expectinguppercase == 1)
		{
			if (name[i] < 'A' || name[i] > 'Z') return 4;
			expectinguppercase = 0;
			continue;
		}
		if (name[i] == '_')
		{
			if (underscorecount == 1) return 5;
			else
			{
				underscorecount = 1;
				expectinguppercase = 1;
			}
			continue;
		}
		if (strval(name[i]) || name[i] == '0' || name[i] == ' ' || name[i] == '[' || name[i] == ']') return 6;
	}
	new namesplit[2][32];
	split(name, namesplit, '_');
	if (strlen(namesplit[0]) < 2 || strlen(namesplit[1]) < 2) return 7;
	//if (strcmp(PlayerName(playerid), "CARRY", true) == 0) return 8;
	return 1;
}

CheckIP(const ip[])
{
	if(15 < strlen(ip))
	{
	    return 2;
	}
	if(strlen(ip) < 7)
	{
	    return 3;
	}
	for (new i = 0, j = strlen(ip); i < j; i++)
	{
	    if(ip[i] != '.' && ip[i] != '0' && ip[i] != '1' && ip[i] != '2' && ip[i] != '3'
		&& ip[i] != '4' && ip[i] != '5' && ip[i] != '6' && ip[i] != '7' && ip[i] != '8' && ip[i] != '9')
		{
		    return 4;
		}
	}
	new ipsplit[4][32];
	split(ip, ipsplit, '.');
	if(!strlen(ipsplit[0]) || !strlen(ipsplit[1]) || !strlen(ipsplit[2]) || !strlen(ipsplit[3])) return 9;
	if((strlen(ipsplit[0])<0 || strlen(ipsplit[0])>3 ) || (strval(ipsplit[0])<0 || strval(ipsplit[0])>255)) return 5;
	if((strlen(ipsplit[1])<0 || strlen(ipsplit[1])>3 ) || (strval(ipsplit[1])<0 || strval(ipsplit[1])>255)) return 6;
	if((strlen(ipsplit[2])<0 || strlen(ipsplit[2])>3 ) || (strval(ipsplit[2])<0 || strval(ipsplit[2])>255)) return 7;
