stock AddRentVehicle(modelid,Float:x,Float:y,Float:z,Float:a,color1,color2,jtype,ltype,spawntype,rmoney=0)
{
	new ve=CreateVehicle(modelid,Float:x,Float:y,Float:z,Float:a,color1,color2,VehicleRespanTime);
	RentInfo[ve][rJobType]=jtype;
	RentInfo[ve][rLicType]=ltype;
	RentInfo[ve][rSpawnType]=spawntype;
	RentInfo[ve][rRentUser]=9999;
	RentInfo[ve][rRentTime]=0;
	RentInfo[ve][rRentMoney]=rmoney;
	VehicleInfo[ve][vHP]=1000;
	VehicleInfo[ve][vPHP]=1000;
	VehicleInfo[ve][vGas]=100;
	if(RentInfo[ve][rJobType]!=0)
	{
        SetVehicleNumberPlate(ve,"직업차량");
	}else if(RentInfo[ve][rLicType]!=0)
	{
	    switch(RentInfo[ve][rLicType])
	    {
			case 1: SetVehicleNumberPlate(ve,"면허시험용");
			case 2: SetVehicleNumberPlate(ve,"면허시험용");
			case 3: SetVehicleNumberPlate(ve,"면허시험용");
			case 4:	SetVehicleNumberPlate(ve,"면허시험용");
		}
	}else{
		SetVehicleNumberPlate(ve,"렌트 차량");
	}
	SetVehicleUpdate(ve);
	return ve;
}

