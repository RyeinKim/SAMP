public LoadFaction(id)
{
	new string[256];
	format(string,sizeof(string),"Faction/%d.ini",id);
	if(INI_Exist(string))
	{
		INI_Open(string,Read);
		FactionInfo[id][fCreate]=strval(INI_Read("fCreate"));
		strmid(FactionInfo[id][fName],INI_Read("fName"),0,strlen(INI_Read("fName")),256);
		FactionInfo[id][fType]=strval(INI_Read("fType"));
		FactionInfo[id][ffX]=floatstr(INI_Read("fX"));
		FactionInfo[id][ffY]=floatstr(INI_Read("fY"));
		FactionInfo[id][ffZ]=floatstr(INI_Read("fZ"));
		FactionInfo[id][ffA]=floatstr(INI_Read("fA"));
		FactionInfo[id][fInterior]=strval(INI_Read("fInterior"));
		FactionInfo[id][fVirtualWorld]=strval(INI_Read("fVirtualWorld"));
		FactionInfo[id][fSafe]=strval(INI_Read("fSafe"));
		for(new f=0;f<MAX_FACTIONRANK;f++)
		{
		    format(string,sizeof(string),"FactionRankName%d",f);
			strmid(FactionRankName[id][f],INI_Read(string),0,strlen(INI_Read(string)),256);
		}
		for(new fv=0;fv<MAX_FVEHICLES;fv++)
		{
			format(string,sizeof(string),"fPlate%d",fv);
			FactionInfo[id][fPlate][fv]=strval(INI_Read(string));
			format(string,sizeof(string),"fVehicleModel%d",fv);
			FactionInfo[id][fVehicleModel][fv]=strval(INI_Read(string));
			format(string,sizeof(string),"fVehicleSpawner%d",fv);
			FactionInfo[id][fVehicleSpawner][fv]=strval(INI_Read(string));
		}
		INI_Close();
		for(new fv=0;fv<MAX_FVEHICLES;fv++)
		{
			if(FactionInfo[id][fVehicleSpawner][fv]==1)
			{
	    		if(FactionInfo[id][fPlate][fv]!=0)
				{
					format(string,sizeof(string),"Vehicle/%d.ini",FactionInfo[id][fPlate][fv]);
					if(INI_Exist(string))
					{
						FactionInfo[id][fVehicle][fv]=LoadVehicle(FactionInfo[id][fPlate][fv]);
					}
				}
			}
		}
	}
}

