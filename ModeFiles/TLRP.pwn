stock SendAdminMessage(color,string[],type=0)
{
	for(new i = 0; i < M_P; i++)
	{
		if(IsPlayerConnected(i))
		{
		    if(togadmin[i]==1)
		    {
				if(type!=0)
				{
					SCM(i, color, string);
				}else{
					if(PlayerInfo[i][pAdmin] >= 1)
					{
						SCM(i, color, string);
					}
				}
			}
		}
	}
	printf(string);
	return 1;
}

