stock CheckMail(inputtext[])
{
	new ch[2];
	for(new i = 0; i < strlen(inputtext); i++)
	{
	    if(inputtext[i] == '@')
	    {
	        if(i == 0)
	        {
	            return -2;
	        }
	        ch[0] ++;
	    }
	    if(ch[0] > 1)
	    {
	        return -1;
	    }
	    if(ch[0])
	    {
		    if(inputtext[i] == '.')
		    {
		        ch[1] ++;
		    }
	    }
	}
	if(!ch[1])
	{
	    return -3;
	}
    new tmp[2][512];
	split(inputtext, tmp, '@');
	for(new i = 0; i < sizeof(AcessMailAdress); i++)
	{
	    if(strcmp(AcessMailAdress[i], tmp[1], true) != 0)
	        return -4;
	}
	return 1;
}

