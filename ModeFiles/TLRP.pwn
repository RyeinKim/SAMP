public OnVehicleSpawn(vehicleid)
{
    SetVehicleHealth(vehicleid, VehicleInfo[vehicleid][vHP]);
	if(SirenVehicle[vehicleid]==1)
	{
	    SirenVehicle[vehicleid]=0;
	    strdel(SirenURL[vehicleid],0,256);
	    DestroyDynamicObject(SirenObject[vehicleid]);
	}
	if(IsAHelicopter(vehicleid))
	{
		if(RappelVehicle[vehicleid]!=999)
		{
	  		DestroyDynamicObject(RappelObject[vehicleid]);
		}
	    RappelVehicle[vehicleid]=999;
	}
	if(VehicleInfo[vehicleid][vModel] != 0)
	{
		ChangeVehicleColor(vehicleid,VehicleInfo[vehicleid][vColor1],VehicleInfo[vehicleid][vColor2]);
		for(new i = 0;i < 17;i++)
		{
			if(i==5 && Nitroused[vehicleid]==true)
			{
				continue;
			}
			AddVehicleComponent(vehicleid,VehicleInfo[vehicleid][vTune][i]);
		}
		if(VehicleInfo[vehicleid][vPaintJob] != 999)
		{
			ChangeVehiclePaintjob(vehicleid, VehicleInfo[vehicleid][vPaintJob]);
		}
		SetVehiclePos(vehicleid,VehicleInfo[vehicleid][vPark_x],VehicleInfo[vehicleid][vPark_y],VehicleInfo[vehicleid][vPark_z]);
		SetVehicleZAngle(vehicleid,VehicleInfo[vehicleid][vPark_r]);
		SetVehicleVirtualWorld(vehicleid,VehicleInfo[vehicleid][vVir]);
	}
	if(VehicleInfo[vehicleid][vVir]!=0)
	{
		SetVehicleVirtualWorld(vehicleid,VehicleInfo[vehicleid][vVir]);
	}
	if(RentInfo[vehicleid][rRentUser]!=9999)
	{
	    SCM(RentInfo[vehicleid][rRentUser],COLOR_LIGHTRED,"렌트 차량이 리스폰되어 렌트가 종료되었습니다.");
	    RentCar[RentInfo[vehicleid][rRentUser]]=0;
	    if(RentInfo[vehicleid][rSpawnType]==1)
	    {
			RentInfo[vehicleid][rRentUser]=9999;
			RentInfo[vehicleid][rRentTime]=0;
			SerVehicleHP(vehicleid,1000);
	    }else{
			OnDestroyVehicle(vehicleid);
			ResetVehicle(vehicleid);
	    }
	}
    SetVehicleUpdate(vehicleid);
	if(strlen(VehicleInfo[vehicleid][vOwner]))
	{
		new check=0;
		for(new i=0;i<M_P;i++)
		{
		    if(IsPlayerConnected(i) && strcmp(GetName(i),VehicleInfo[vehicleid][vOwner],true)==0)
		    {
		        check=1;
		        break;
		    }
		}
		if(check==0)
		{
			SaveVehicle(vehicleid);
			OnDestroyVehicle(vehicleid);
			ResetVehicle(vehicleid);
		}
	}
	return 1;
}

