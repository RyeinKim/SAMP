public OnPlayerConnect(playerid)
{
	ResetVariable(playerid);
	SetPlayerColor(playerid, COLOR_SPEC);
	if(CheckPlayerName(GetName(playerid)) != 1)
	{
	    SCM(playerid,COLOR_WHITE,"당신의 닉네임은 서버에서 사용이 불가능한 닉네임입니다.");
		KickEx(playerid);
		return 1;
	}
	new playersip[256],string[256],mail[256];
    new bandata[256],banadmin[256],banreason[256];
	new ver[40];
	GetPlayerVersion(playerid,ver,sizeof(ver));
	if(strfind(ver,"0.3z",true)==-1)
	{
	    format(string,sizeof(string),"당신의 클라이언트 버전은 '%s'입니다.",ver);
