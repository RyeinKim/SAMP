public GetWeaponType(weaponid)
{
	new WeaponType;
	if(weaponid>=1&&weaponid<=15) WeaponType=1;
	else if(weaponid>=22&&weaponid<=24) WeaponType=2;
	else if(weaponid>=25&&weaponid<=27) WeaponType=3;
	else if(weaponid==28||weaponid==29||weaponid==32) WeaponType=4;
	else if(weaponid==30||weaponid==31||weaponid==33||weaponid==34) WeaponType=5;
	else if(weaponid>=35&&weaponid<=38) WeaponType=6;
	else if(weaponid==41||weaponid==42) WeaponType=7;
	else if(weaponid>=16&&weaponid<=18) WeaponType=8;
	else if(weaponid==39||weaponid==40||(weaponid>=43&&weaponid<=46)) WeaponType=9;
	else WeaponType=0;
	return WeaponType;
}

stock SetVehicleUpdate(vehicleid)
{
    KillTimer(UpdateVehicleTimer[vehicleid]);
    KillTimer(UpdateVehicleTimer2[vehicleid]);
    UpdateVehicleTimer[vehicleid]=SetTimerEx("OnVehicleUpdate",300,true,"i",vehicleid);
    UpdateVehicleTimer2[vehicleid]=SetTimerEx("OnVehicleUpdate1000",1000,true,"i",vehicleid);
}

stock OnDestroyVehicle(vehicleid)
{
	if(VehicleInfo[vehicleid][vTaxiCab]==1)
	{
		DestroyDynamicObject(VehicleTaxiCab[vehicleid]);
	}
	if(SirenVehicle[vehicleid]==1)
	{
	    SirenVehicle[vehicleid]=0;
	    strdel(SirenURL[vehicleid],0,256);
	    DestroyDynamicObject(SirenObject[vehicleid]);
	}
    if(GetVehicleModel(vehicleid)==544)
    {
        if(IsValidDynamicObject(Ladder[vehicleid]))
        {
		    DestroyDynamicObject(Ladder[vehicleid]);
		}
	}
	if(IsAHelicopter(vehicleid))
	{
		if(RappelVehicle[vehicleid]!=999)
		{
	  		DestroyDynamicObject(RappelObject[vehicleid]);
		}
	    RappelVehicle[vehicleid]=999;
	}
	DestroyVehicle(vehicleid);
	return 1;
}

