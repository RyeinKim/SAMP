forward Nitrodown();
public Nitrodown()
{
	for(new vehicleid=1;vehicleid<MAX_VEHICLES;vehicleid++)
	{
		if(Nitroused[vehicleid]==true&&NitroPower[vehicleid]>=1&&Nitrouse[vehicleid]==true)
		{
            NitroPower[vehicleid]--;
            if(NitroPower[vehicleid]<=0)
           	{
    			Nitrouse[vehicleid]=false;
                NitroPower[vehicleid]=0;
       		    RemoveVehicleComponent(vehicleid, 1010);
			}
		}
	}
}

forward Nitroup();
public Nitroup()
{
	for(new vehicleid=1;vehicleid<MAX_VEHICLES;vehicleid++)
	{
		if(Nitroused[vehicleid]==true&&NitroPower[vehicleid]<100&&Nitrouse[vehicleid]==false)
		{
            NitroPower[vehicleid]++;
            if(NitroPower[vehicleid]>=100)
           	{
                NitroPower[vehicleid]=100;
			}
		}
	}
}

stock SendClientMSG(playerid,color,const string[])
{
    if( IsPlayerConnected(playerid) )
    {
        new eng=0,kor=0,check=0,tmpstring[128];
        if(strlen(string)<=128)
        {
	        SendClientMessage(playerid,color,string);
	        return 1;
		}
	    for(new i=0,j=strlen(string);;i++)
        {
            if(string[i]!=0 && string[i]<128)
            {
			    eng++;
			}else if(string[i]!=0 ){
			    kor++;
			}
			if(kor%2==0 && eng+kor>=123 && check==0)
			{
				check=eng+kor;
				strmid(tmpstring,string,0,check,128);
				format(tmpstring,sizeof(tmpstring),"%s..",tmpstring);
		    	SendClientMessage(playerid,color,tmpstring);
			}
            if( i==j )
            {
				strmid(tmpstring,string,check,j,128);
				format(tmpstring,sizeof(tmpstring),"..%s",tmpstring);
		    	SendClientMessage(playerid,color,tmpstring);
		    	break;
            }
		}
	}
	return 1;
}

