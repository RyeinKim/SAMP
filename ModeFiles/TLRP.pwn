stock MoveLadder(vehicleid, Float:UD,Float:LR)
{
	new
		Float:x,Float:y,Float:z,
		Float:tx,Float:ty,Float:tz,
		Float:radius = 12.8
	;
	x = 0.0;
	//y = 9.0-radius;
	y = 13.0-radius;
	z = 1.8;
	Ladder_UD[vehicleid] += UD;
	Ladder_LR[vehicleid] -= LR;
	if(Ladder_UD[vehicleid] <= 0)
		Ladder_UD[vehicleid] = 0;
	else if(Ladder_UD[vehicleid] > 70)
		Ladder_UD[vehicleid] = 70;
	if(Ladder_LR[vehicleid] < -360)
		Ladder_LR[vehicleid] = -0;
	else if(Ladder_LR[vehicleid] > 360)
		Ladder_LR[vehicleid] = 0;
	tx = -floatsin(Ladder_LR[vehicleid],degrees) * floatcos(Ladder_UD[vehicleid],degrees);
	ty = floatcos(Ladder_LR[vehicleid],degrees) * floatcos(Ladder_UD[vehicleid],degrees);
	tz = floatsin(Ladder_UD[vehicleid],degrees);
	x += radius * tx;
	y += radius * ty;
	z += radius * tz;
	AttachDynamicObjectToVehicle(Ladder[vehicleid], vehicleid, x,y,z, Ladder_UD[vehicleid],90.0,Ladder_LR[vehicleid]);
	return 1;
}

