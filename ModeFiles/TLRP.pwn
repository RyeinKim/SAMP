stock GiveItem(playerid,item,amount,amount2)
{
	if(PockInfo[item][poTypes]==0)
	{
		for(new i=0;i<MAX_POCKET;i++)
		{
		    if(amount>0 && PlayerInfo[playerid][pPocketType][i]==0)
			{
                PlayerInfo[playerid][pPocketType][i]=item;
                PlayerInfo[playerid][pPocketAmount][i]=amount;
                PlayerInfo[playerid][pPocketAmount2][i]=amount2;
		        if(PlayerInfo[playerid][pPocketAmount][i]<=0)
		        {
			        PlayerInfo[playerid][pPocketType][i]=0;
	                PlayerInfo[playerid][pPocketAmount][i]=0;
         		    PlayerInfo[playerid][pPocketAmount2][i]=0;
				}
				return 1;
            }
			if(amount<=0 && PlayerInfo[playerid][pPocketType][i]==item)
			{
                PlayerInfo[playerid][pPocketType][i]=item;
                PlayerInfo[playerid][pPocketAmount][i]=amount;
                PlayerInfo[playerid][pPocketAmount2][i]=amount2;
		        if(PlayerInfo[playerid][pPocketAmount][i]<=0)
		        {
			        PlayerInfo[playerid][pPocketType][i]=0;
	                PlayerInfo[playerid][pPocketAmount][i]=0;
         		    PlayerInfo[playerid][pPocketAmount2][i]=0;
				}
				return 1;
            }
		}
	}else{
	    new check=0;
		for(new i=0;i<MAX_POCKET;i++)
		{
		    if(PlayerInfo[playerid][pPocketType][i]==item)
			{
                PlayerInfo[playerid][pPocketType][i]=item;
                PlayerInfo[playerid][pPocketAmount][i]+=amount;
                PlayerInfo[playerid][pPocketAmount2][i]=amount2;
		        if(PlayerInfo[playerid][pPocketAmount][i]<=0)
		        {
			        PlayerInfo[playerid][pPocketType][i]=0;
	                PlayerInfo[playerid][pPocketAmount][i]=0;
         		    PlayerInfo[playerid][pPocketAmount2][i]=0;
				}
				check=1;
				return 1;
            }
		}
		if(check==0)
		{
			for(new i=0;i<MAX_POCKET;i++)
			{
			    if(PlayerInfo[playerid][pPocketType][i]==0)
				{
	                PlayerInfo[playerid][pPocketType][i]=item;
	                PlayerInfo[playerid][pPocketAmount][i]+=amount;
	                PlayerInfo[playerid][pPocketAmount2][i]=amount2;
			        if(PlayerInfo[playerid][pPocketAmount][i]<=0)
			        {
				        PlayerInfo[playerid][pPocketType][i]=0;
	         		    PlayerInfo[playerid][pPocketAmount2][i]=0;
					}
					return 1;
                }
			}
		}
	}
	return 0;
}

stock GetPlayerFPS(playerid)
{
