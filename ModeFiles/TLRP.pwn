{
	for(new playerid; playerid < M_P; playerid++)
	{
		if(IsPlayerConnected(playerid))
		{
			if(!IsPlayerNPC(playerid))
			{
				new keys[3];
				new Pissing = Pissing_at_Flame(playerid);
				new Aiming = Aiming_at_Flame(playerid);
				GetPlayerKeys(playerid, keys[0], keys[1], keys[2]);
				if(PlayerOnFire[playerid] && !CanPlayerBurn(playerid, 1))
				{ StopPlayerBurning(playerid); }
				if(Pissing != -1 || Aiming != -1)
				{
					if(ExtTimer[playerid] == -1 && ((Aiming != -1 && keys[0] & KEY_FIRE) || Pissing != -1))
					{
						new value, time, Float:x, Float:y, Float:z;
						if(Pissing != -1)
						{
							value = Pissing;
							time = EXTINGUISH_TIME_PEEING;
						}
						else if(Aiming != -1)
						{
							value = Aiming;
//							if(GetPlayerWeapon(playerid) == 41)
//							{
//								CreateExplosion(Flame[value][Flame_x], Flame[value][Flame_y], Flame[value][Flame_z], 2, 5);
//								continue;
//							}
							if(IsPlayerInAnyVehicle(playerid))
							{
								time = EXTINGUISH_TIME_VEHICLE;
							}
							else
							{
								time = EXTINGUISH_TIME_ONFOOT;
							}
						}
						if(value < -1) { time = EXTINGUISH_TIME_PLAYER; }
						time *= 1000;
						if(value >= -1)
						{
							x = Flame[value][Flame_x];
							y = Flame[value][Flame_y];
							z = Flame[value][Flame_z];
							DestroyTheSmokeFromFlame(value);
							Flame[value][Smoke] = CreateDynamicObject(18727, x, y, z, 0.0, 0.0, 0.0);
//							Flame[value][Smoke][1] = CreateDynamicObject(18727, x+1, y, z, 0.0, 0.0, 0.0);
