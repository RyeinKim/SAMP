							{
							    SetVehicleBurn(GetPlayerVehicleID(playerid));
								return 1;
							}
						}
					}
				}
				/*#if defined BurnOthers
				new Float:x, Float:y, Float:z;
				for(new i; i < M_P; i++)
			  	{
			  		if(playerid != i && IsPlayerConnected(i) && !IsPlayerNPC(i))
