public OnPlayerExitVehicle(playerid, vehicleid)
{
	if(TestType[playerid]!=0)
	{
		if(TestCar[playerid]!=0)
		{
		    if(RentInfo[TestCar[playerid]][rSpawnType]==1)
		    {
                SerVehicleHP(TestCar[playerid],1000);
			    SetVehicleToRespawn(TestCar[playerid]);
		    }else{
				OnDestroyVehicle(TestCar[playerid]);
				ResetVehicle(TestCar[playerid]);
		    }
			FailCheckPoint(playerid,TestCar[playerid],0);
			SCM(playerid,COLOR_LIGHTRED,"당신은 차량에서 내려서 시험에서 탈락했습니다.");
		}
	}
	if(RappelVehicle[vehicleid] != 999)
	{
	    RappelPlayerid[playerid]=vehicleid;
	}
	return 1;
}

public OnPlayerStateChange(playerid, newstate, oldstate)
{
	new vehicleid = GetPlayerVehicleID(playerid),string[256];
	if(PlayerHaul[playerid]!=999)
	{
		if(oldstate == PLAYER_STATE_PASSENGER)
		{
		    new tmpid=PlayerHaul[playerid];
			PlayerHaul[playerid]=999;
			PutPlayerInVehicle(tmpid,gLastCar[playerid],PlayerSeat[playerid]);
			format(string, sizeof(string), " %s이(가) %s를 차량에 태운다.", PlayerName(playerid), PlayerName(tmpid));
			BProxDetector(15.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		}
	}
	if(newstate == PLAYER_STATE_PASSENGER || newstate == PLAYER_STATE_DRIVER)
	{
	    PlayerSeat[playerid] = GetPlayerVehicleSeat(playerid);
	    gLastCar[playerid] = vehicleid;
	}
	if(oldstate == PLAYER_STATE_DRIVER && newstate == PLAYER_STATE_ONFOOT)
	{
	    if(PlayerSpeedLimit[playerid]!=0)
	    {
			PlayerSpeedLimit[playerid]=0;
			SCM(playerid,COLOR_LIGHTRED,"당신은 차에서 내려 속도제한이 풀립니다.");
		}
	}
	if(newstate == PLAYER_STATE_DRIVER)
	{
	    if(	RentInfo[vehicleid][rJobType]==0 && RentInfo[vehicleid][rLicType]==0 && RentInfo[vehicleid][rSpawnType]==0)
	    {
			format(string, sizeof(string), "차량번호판 [ %d ]",VehicleInfo[vehicleid][vSign]);
		}else if( RentInfo[vehicleid][rJobType]!=0 )
		{
		    format(string, sizeof(string), "차량번호판 [ 직업차량 ]");
		}else if( RentInfo[vehicleid][rLicType]!=0 )
		{
		    format(string, sizeof(string), "차량번호판 [ 면허실습차량 ]");
		}else if( RentInfo[vehicleid][rSpawnType]!=0 )
		{
		    format(string, sizeof(string), "차량번호판 [ 렌트차량 ]");
		}
		SCM(playerid,COLOR_WHITE, string);
		new engine,lights,alarm,doors,bonnet,boot,objective;
		GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
		if(IsBicycle(GetVehicleModel(vehicleid)))
		{
			SetVehicleParamsEx(vehicleid,1,0,alarm,doors,bonnet,boot,objective);
		}
		if(RentInfo[vehicleid][rLicType]==0 || TestType[playerid]==0 )
		{
			if(IsShip(GetVehicleModel(vehicleid)))
			{
				if(PlayerInfo[playerid][pBoatLic] < 1)
				{
					SCM(playerid,COLOR_WHITE,"당신은 아직 항해하는 방법을 모릅니다, 보트에서 내렸습니다.");
					RemovePlayerFromVehicle(playerid);
				}
			}
			else if(IsAirPlane(GetVehicleModel(vehicleid)))
			{
				if(PlayerInfo[playerid][pAirLic] < 1)
				{
					SCM(playerid,COLOR_WHITE,"당신은 아직 비행하는 방법을 모릅니다, 비행기에서 내렸습니다.");
					RemovePlayerFromVehicle(playerid);
				}
			}
		}
		if(engine == 1)
		{
			SCM(playerid,COLOR_WHITE,"차량의 시동이 걸려 있습니다.");
		}
		else if(!IsABike(vehicleid))
		{
			SCM(playerid,COLOR_WHITE,"차량의 시동이 걸려있지 않습니다.");
		}
		if(RentInfo[vehicleid][rRentMoney]!=0 && RentInfo[vehicleid][rRentUser]==9999)
		{
		    format(string,sizeof(string),"이 렌트 차량을 빌릴려면 $%d가 필요합니다.",RentInfo[vehicleid][rRentMoney]);
		    SCM(playerid,COLOR_WHITE,string);
		    ShowPlayerDialog(playerid,DIALOG_30,DIALOG_STYLE_LIST,"렌트카","대여\n반납\n시간","확인","취소");
		}
		if(RentInfo[vehicleid][rRentUser]!=playerid && RentInfo[vehicleid][rRentUser]!=9999)
		{
		    SCM(playerid,COLOR_WHITE,"다른 사람이 이 렌트 차량을 대여했습니다.");
		    RemovePlayerFromVehicle(playerid);
		}
		if(VehicleFaction[vehicleid]!=0)
		{
		    if(PlayerInfo[playerid][pFaction]!=VehicleFaction[vehicleid])
			{
		        SCM(playerid,COLOR_WHITE,"당신은 이 차량을 사용할 권한이 없습니다.");
				RemovePlayerFromVehicle(playerid);
			}
		}
		if(PlayerCuffed[playerid]==1)
		{
