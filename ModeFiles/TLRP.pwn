				{
				    DisableVehicleAlarm(vehicleid);
				}else if(RentInfo[vehicleid][rSpawnType]==0 && RentInfo[vehicleid][rRentMoney]==0){
					GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
				    SetVehicleParamsEx(vehicleid,engine,lights,true,doors,bonnet,boot,objective);
				    UpdateVehicleTimer3[vehicleid]=SetTimerEx("DisableVehicleAlarm",20000,false,"i",vehicleid);
				}
			}else if((VehicleFaction[vehicleid]!=0 && VehicleFaction[vehicleid] != PlayerInfo[playerid][pFaction]))
			{
				GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
			    SetVehicleParamsEx(vehicleid,engine,lights,true,doors,bonnet,boot,objective);
			    UpdateVehicleTimer3[vehicleid]=SetTimerEx("DisableVehicleAlarm",20000,false,"i",vehicleid);
			}else if((VehicleFaction[vehicleid]!=0 && VehicleFaction[vehicleid] == PlayerInfo[playerid][pFaction]))
			{
			    if(UpdateVehicleTimer3[vehicleid]!=0)
			    {
				    DisableVehicleAlarm(vehicleid);
				}
			}
		}
 	}
	return 1;
}

