public FailCheckPoint(playerid,vehicleid,type)
{
	TestTimer[playerid]=0;
	SerVehicleHP(vehicleid,1000);
	SetVehicleToRespawn(vehicleid);
	TestType[playerid]=0;
	WorkType[playerid]=0;
	if(type==0)
	{
		TestCar[playerid]=0;
