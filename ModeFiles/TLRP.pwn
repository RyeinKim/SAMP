stock IsPlayerFront(playerid,targetid)
{
	new Float:a0,Float:a1,Float:sum;
	GetPlayerFacingAngle(playerid,a0),GetPlayerFacingAngle(targetid,a1);
	sum=a0-a1;
	floatabs(sum);
	if(135<sum && sum<225)
	{
	    return 1;
	}
	return 0;
}

stock BallCreate(id,Float:x,Float:y,Float:z)
{
	if(IsValidDynamicObject(BasketInfo[id][Ball]))
	{
		DestroyDynamicObject(BasketInfo[id][Ball]);
	}
    BasketInfo[id][Ball]=CreateDynamicObject(BALL_ID,x,y,z,0,0,0);
}

stock BallPlayerPass(playerid)
{
    new
		Float:pos[4],
		i=PlayerBasketBall[playerid]
	;
	for(new p=0;p<M_P;p++)
	{
	    if(IsPlayerConnected(p))
	    {
			if(BallTeam[playerid]!=9999 && BallTeam[p]==BallTeam[playerid] && PlayerBasketBall[p]==PlayerBasketBall[playerid]&& playerid!=p)
			{
				if(GetDistanceBetweenPlayers(playerid,p) < 4.5)
				{
					new String[128];
				    GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
					BallCreate(i,pos[0], pos[1], pos[2]);
					GetPlayerFacingAngle(playerid, pos[3]);
				    GetPlayerPos(p, pos[0], pos[1], pos[2]);
					MoveDynamicObject(BasketInfo[i][Ball],pos[0], pos[1], pos[2],8);
					Anim(playerid, "BSKTBALL", "BBALL_react_score", 4.1, 0, 1, 1, 0, 0);
					PlayerPickupBall[playerid] = 9999;
					RemovePlayerAttachedObject(playerid, BALL_INDEX);
				    BasketInfo[i][BallPickup] = -1;
				    BasketInfo[i][RunPlayer] = 0;
				    BasketInfo[i][BallGoal] = 0;
				    BasketInfo[i][BallCheck] = 0;
					format(String, sizeof(String), " %s이(가) %s에게 패스를 한다.", PlayerName(playerid),PlayerName(p));
					BProxDetector(15.0, playerid, String, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				    SetTimerEx("BallPassed", 400, false, "i", p);
				    break;
				}
			}
		}
	}
}

stock ShootBall(playerid)
{
    new
		Float:pos[4],
		i=PlayerBasketBall[playerid]
	;
	GetPlayerPos(playerid, pos[0], pos[1], pos[2]);

	if( IsPlayerInRangeOfPoint(playerid, 0.9, BasketInfo[i][GoalDunk1][0],BasketInfo[i][GoalDunk1][1],BasketInfo[i][GoalDunk1][2]) ||  IsPlayerInRangeOfPoint(playerid, 0.9, BasketInfo[i][GoalDunk2][0],BasketInfo[i][GoalDunk2][1],BasketInfo[i][GoalDunk2][2]) )
	{
		BallCreate(PlayerBasketBall[playerid],pos[0], pos[1], pos[2]);

        Anim(playerid, "BSKTBALL", "BBALL_Dnk_Gli", 4.1, 0, 1, 1, 0, 0);
        if( IsPlayerInRangeOfPoint(playerid, 4.0, BasketInfo[i][GoalDunk1][0],BasketInfo[i][GoalDunk1][1],BasketInfo[i][GoalDunk1][2]))
        {
        	SetPlayerToFaceBall(playerid,BasketInfo[i][BPost][0]);
			MoveDynamicObject(BasketInfo[i][Ball],BasketInfo[i][GoalDunk1][0],BasketInfo[i][GoalDunk1][1],BasketInfo[i][GoalDunk1][2]+3.0, 7);
        }else{
        	SetPlayerToFaceBall(playerid,BasketInfo[i][BPost][1]);
			MoveDynamicObject(BasketInfo[i][Ball],BasketInfo[i][GoalDunk2][0],BasketInfo[i][GoalDunk2][1],BasketInfo[i][GoalDunk2][2]+3.0, 7);
        }
        SetTimerEx("OnPlayerDunk", 300, false, "i", playerid);
		BasketInfo[i][BallGoal] = 1;

		RemovePlayerAttachedObject(playerid, BALL_INDEX);
	    BasketInfo[i][BallPickup] = -1;
	    PlayerPickupBall[playerid] = 9999;
	    BasketInfo[i][RunPlayer] = 0;
	    BasketInfo[i][BallCheck] = 0;
	    BasketInfo[i][BallPass] = 0;
	    BasketInfo[i][LastShooter] = playerid;
	} else if(IsPlayerInRangeOfPoint(playerid, 7.0, BasketInfo[i][GoalDunk1][0],BasketInfo[i][GoalDunk1][1],BasketInfo[i][GoalDunk1][2]))
	{
		BallCreate(PlayerBasketBall[playerid],pos[0], pos[1], pos[2]);

       	SetPlayerToFaceBall(playerid,BasketInfo[i][BPost][0]);

		Anim(playerid, "BSKTBALL", "BBALL_Jump_Shot", 4.1, 0, 1, 1, 0, 0);
		MoveDynamicObject(BasketInfo[i][Ball],BasketInfo[i][GoalDunk1][0],BasketInfo[i][GoalDunk1][1],BasketInfo[i][GoalDunk1][2]+3.0, 7);
		BasketInfo[i][BallGoal] = 2;

		RemovePlayerAttachedObject(playerid, BALL_INDEX);
	    BasketInfo[i][BallPickup] = -1;
	    PlayerPickupBall[playerid] = 9999;
	    BasketInfo[i][RunPlayer] = 0;
	    BasketInfo[i][BallCheck] = 0;
	    BasketInfo[i][BallPass] = 0;
	    BasketInfo[i][LastShooter] = playerid;
	} else if(IsPlayerInRangeOfPoint(playerid, 7.0, BasketInfo[i][GoalDunk2][0],BasketInfo[i][GoalDunk2][1],BasketInfo[i][GoalDunk2][2])){
		BallCreate(PlayerBasketBall[playerid],pos[0], pos[1], pos[2]);

       	SetPlayerToFaceBall(playerid,BasketInfo[i][BPost][1]);

		Anim(playerid, "BSKTBALL", "BBALL_Jump_Shot", 4.1, 0, 1, 1, 0, 0);
		MoveDynamicObject(BasketInfo[i][Ball],BasketInfo[i][GoalDunk2][0],BasketInfo[i][GoalDunk2][1],BasketInfo[i][GoalDunk2][2]+3.0, 7);
		BasketInfo[i][BallGoal] = 2;

