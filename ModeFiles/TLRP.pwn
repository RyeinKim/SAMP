					{
						idx++;
					}
					new offset = idx;
					new result[256];
					while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
					{
						result[idx - offset] = cmdtext[idx];
						idx++;
					}
					result[idx - offset] = EOS;
					if(!strlen(result))
					{
						SCM(playerid,COLOR_WHITE, "/신고 [플레이어번호/이름의부분] [이유]");
						return 1;
					}
					format(string, sizeof(string), "제보자: %s(%d) , 신고대상: %s(%d) , {E60000}이유: %s",PlayerName(playerid), playerid, PlayerName(giveplayerid), giveplayerid, (result));
					SendAdminMessage(COLOR_WARNING,string);
					SCM(playerid,COLOR_WHITE, "당신의 신고 요청은  어드민에게 전송되었습니다.");
				}
			}
