			SendAdminMessage(0xC0E0E1FF,string);
		}
		return 1;
	}

	if(strcmp(cmd, "/신고", true) == 0||!strcmp("/report",cmd,true))
	{
		if(IsPlayerConnected(playerid))
		{
			tmp = strtok(cmdtext, idx);
