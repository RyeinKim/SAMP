public CheckLadderMoving()
{
	new vehicleid;
	for(new i; i<M_P; i++)
		if(IsPlayerInAnyVehicle(i))
		{
			vehicleid = GetPlayerVehicleID(i);
			if(IsValidDynamicObject(Ladder[vehicleid]))
			{
				if(GetVehicleModel(vehicleid) == 544)
				{
					new key,a,b;
					GetPlayerKeys(i,key,a,b);
					if(key & KEY_ANALOG_UP)
						MoveLadder(vehicleid, 0.4, 0.0);
					else if(key & KEY_ANALOG_DOWN)
						MoveLadder(vehicleid, -0.4, 0.0);
					if(key & KEY_ANALOG_LEFT)
						MoveLadder(vehicleid, 0.0, -0.4);
					else if(key & KEY_ANALOG_RIGHT)
						MoveLadder(vehicleid, 0.0, 0.4);
				}
			}
		}
	return 1;
}

