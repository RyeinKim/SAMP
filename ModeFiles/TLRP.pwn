stock SetPlayerToFaceBall(playerid,objectid)
{
	new
		Float: PlayersPos[3],
		Float: Pos[3], Float: BallAngle;

	if( IsPlayerConnected(playerid) )
	{
		GetDynamicObjectPos(objectid, Pos[0], Pos[1], Pos[2]);
		GetPlayerPos(playerid, PlayersPos[0], PlayersPos[1], PlayersPos[2]);

		if( Pos[1] > PlayersPos[1] )
		{
			BallAngle = ( -acos((Pos[0] - PlayersPos[0]) / floatsqroot((Pos[0] - PlayersPos[0]) * \
			(Pos[0] - PlayersPos[0]) + (Pos[1] - PlayersPos[1])*(Pos[1] - PlayersPos[1]))) - 90.0);
		}
		else if( Pos[1] < PlayersPos[1] && Pos[0] < PlayersPos[0] )
		{
			BallAngle = ( acos((Pos[0] - PlayersPos[0]) / floatsqroot((Pos[0] - PlayersPos[0]) * \
			(Pos[0] - PlayersPos[0]) + (Pos[1] - PlayersPos[1])*(Pos[1] - PlayersPos[1]))) - 450.0);
		}
		else if( Pos[1] < PlayersPos[1] )
		{
			BallAngle = ( acos((Pos[0] - PlayersPos[0]) / floatsqroot((Pos[0] - PlayersPos[0]) * \
			(Pos[0] - PlayersPos[0]) + (Pos[1] - PlayersPos[1])*(Pos[1] - PlayersPos[1]))) - 90.0);
		}
		if( Pos[0] > PlayersPos[0] )
		BallAngle = (floatabs( floatabs(BallAngle) + 180.0 ));
		else
		BallAngle = ( floatabs(BallAngle) - 180.0 );
		SetPlayerFacingAngle(playerid, BallAngle);
	}
}

stock ObjectToPoint(objectid, Float:radi, Float:x, Float:y, Float:z)
