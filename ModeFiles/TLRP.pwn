public PlayerTimer501(playerid)
{
	new string[256];
	if(AShowType[playerid]!=0 || FShowType[playerid]!=0 || AType[playerid]!=0)
	{
		Anim(playerid,"PED","facanger",4.1, 1,1,1,1,1);
	}

	if(C4Taking[playerid][0]!=0 || C4Taking[playerid][1]!=0 || C4Taking[playerid][2]!=0)
	{
	    if(!IsPlayerInRangeOfPoint(playerid,2.0,C4Taking[playerid][0],C4Taking[playerid][1],C4Taking[playerid][2]))
	    {
			if(C4TakingType[playerid]==1 && IsPlayerInAnyVehicle(playerid))
			{

			}else{
				KillTimer(KillTimer10[playerid]);
				C4Taking[playerid][0]=0,C4Taking[playerid][1]=0,C4Taking[playerid][2]=0;
				Anim(playerid,"PED","facgum",4.1,0,1,1,1,1);
				SCM(playerid,COLOR_WHITE,"당신은 움직여서 행동을 중지합니다.");
			}
		}
	}
	if(PlayerHaul[playerid]!=999)
	{
	    if(!IsPlayerConnected(PlayerHaul[playerid]) || PlayerInfo[PlayerHaul[playerid]][pDeath] == 0)
	    {
	        PlayerHaul[playerid]=999;
	    }else{
		    new Float:x,Float:y,Float:z,Float:a,vw,interior;
		    GetPlayerPos(playerid,x,y,z);
		    GetPlayerFacingAngle(playerid,a);
			new Float:X1,Float:Y1;
			X1 += x+(floatsin(-a, degrees) * -1.2);
			Y1 += y+(floatcos(-a, degrees) * -1.2);
			interior=GetPlayerInterior(playerid);
			vw=GetPlayerVirtualWorld(playerid);
		    SetPlayerPos(PlayerHaul[playerid],X1,Y1,z);
			SetPlayerInterior(PlayerHaul[playerid],interior);
			SetPlayerVirtualWorld(PlayerHaul[playerid],vw);
		}
	}
	if(TestType[playerid]!=0)
	{
	    if(TestTimer[playerid]!=0)
	    {
			TextDrawHideForPlayer(playerid,LeftTime[playerid]);
			format(string,sizeof(string),"Left Time: %d",TestTimer[playerid]-1);
			TextDrawSetString(LeftTime[playerid],string);
			TextDrawShowForPlayer(playerid,LeftTime[playerid]);
		}
	}else{
	    TextDrawHideForPlayer(playerid,LeftTime[playerid]);
	}
	if(PlayerInfo[playerid][pGps]==1)
	{
	    if(CheckItem(playerid,11)<1)
	    {
			SCM(playerid,COLOR_WHITE,"당신은 GPS기기가 없습니다.");
			PlayerInfo[playerid][pGps]=0;
	    }
		new Zonen[MAX_ZONE_NAME];
		GetPlayer2DZone(playerid, Zonen, MAX_ZONE_NAME);
		TextDrawHideForPlayer(playerid,LocationDraw[playerid]);
		format(string,sizeof(string),"%s",Zonen);
		TextDrawSetString(LocationDraw[playerid],string);
		TextDrawShowForPlayer(playerid,LocationDraw[playerid]);
	}else{
		TextDrawHideForPlayer(playerid,LocationDraw[playerid]);
