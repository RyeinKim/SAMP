public PayDay(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		new string[256];
		new account,interest,checks,checks2;
		new rent = 0;
		new tmpintrate=1;
		account = PlayerInfo[playerid][pBank];
		if(PlayerInfo[playerid][pPremium]>0)
		{
		    checks2+=850;
		}
		if(IsACop(playerid))
		{
		    checks+=PlayerInfo[playerid][pLevel]*(7+random(4));
		    checks+=350;
		}
		else if(IsAFD(playerid))
		{
		    checks+=PlayerInfo[playerid][pLevel]*(7+random(4));
		    checks+=400;
		}else{
		//if(PlayerInfo[playerid][pJob]!=0){
            checks+=150;
		    checks+=PlayerInfo[playerid][pLevel]*(7+random(4));
		}
		Tax+=(checks/100)*TaxValue;
		interest = (PlayerInfo[playerid][pBank]/1000)*(tmpintrate);
		PlayerInfo[playerid][pBank] = (account+interest+checks+checks2)-(rent+((checks/100)*TaxValue));
	    if(PlayerInfo[playerid][pRentHouse]!=9999)
	    {
			if(PlayerInfo[playerid][pBank]<HouseInfo[PlayerInfo[playerid][pRentHouse]][hRentCost])
			{
			    PlayerInfo[playerid][pRentHouse]=9999;
			    SCM(playerid,COLOR_LIGHTRED,"당신은 렌트한 집의 렌트비를 낼 돈이없어 쫓겨났습니다.");
			}else{
			    PlayerInfo[playerid][pBank]-=HouseInfo[PlayerInfo[playerid][pRentHouse]][hRentCost];
                HouseInfo[PlayerInfo[playerid][pRentHouse]][hSafe]+=HouseInfo[PlayerInfo[playerid][pRentHouse]][hRentCost];
			}
		}
		SCM(playerid, COLOR_WHITE, "|___ 은행 계좌 통지서 ___|");
		format(string, sizeof(string), 	  "  급료: $%d   세금: -$%d", checks, ((checks/100)*TaxValue));
		SCM(playerid, COLOR_GRAD1, string);
		if(PlayerInfo[playerid][pPremium]>0)
		{
			format(string, sizeof(string), "  Premium : $850");
			SCM(playerid, COLOR_GREY, string);
		}
		if(PlayerInfo[playerid][pPhoneNumber]!=0)
		{
			PlayerInfo[playerid][pPhoneTax]+=10;
		}
		if(PlayerInfo[playerid][pPhoneTax]>0)
		{
			format(string, sizeof(string), "  전화 청구비: -$%d (기본료: $10)", PlayerInfo[playerid][pPhoneTax]);
			SCM(playerid, COLOR_GREY, string);
			Tax+=PlayerInfo[playerid][pPhoneTax];
			PlayerInfo[playerid][pBank]-=PlayerInfo[playerid][pPhoneTax];
			PlayerInfo[playerid][pPhoneTax]=0;
		}
		if(PlayerInfo[playerid][pRentHouse]!=9999)
		{
			format(string, sizeof(string), "  집 렌트비: -$%d", HouseInfo[PlayerInfo[playerid][pRentHouse]][hRentCost]);
			SCM(playerid, COLOR_GRAD5, string);
		}
		if(PlayerInfo[playerid][pHealTax]>0)
		{
			format(string, sizeof(string), "  치료 청구비 : -$%d", PlayerInfo[playerid][pHealTax]);
			SCM(playerid, COLOR_GRAD5, string);
			Tax+=PlayerInfo[playerid][pHealTax];
			PlayerInfo[playerid][pBank]-=PlayerInfo[playerid][pHealTax];
			PlayerInfo[playerid][pHealTax]=0;
		}
		format(string, sizeof(string), 	  "  잔액: $%d 이자 소득 $%d ",account,interest);
		SCM(playerid, COLOR_GRAD1, string);
		format(string, sizeof(string),    "  이율: 0.%d 퍼센트",tmpintrate);
		SCM(playerid, COLOR_GREY, string);
		SCM(playerid, COLOR_LIGHTRED, "※ 급료가 은행에 입금되었습니다.");
		SCM(playerid, COLOR_GRAD4, "|________________________|");
		format(string, sizeof(string), "  새로운 잔액: $%d", PlayerInfo[playerid][pBank]);
		SCM(playerid, COLOR_GRAD5, string);
		if(PlayerInfo[playerid][pBank] < 0)
		{
		    SCM(playerid, COLOR_LIGHTRED, "세금을 낼 돈이 부족하여 경험치가 감소합니다.");
			PlayerInfo[playerid][pExp]--;
		}else{
		    PlayerInfo[playerid][pExp]++;
			new nxtlevel = PlayerInfo[playerid][pLevel]+1;
			new expamount = nxtlevel*levelexp;
			if (PlayerInfo[playerid][pExp] >= expamount)
			{
			    if(PlayerInfo[playerid][pLevel]<=3)
			    {
       				PlayerInfo[playerid][pLevel]++;
					PlayerInfo[playerid][pExp] = 0;
					SetPlayerScore(playerid,PlayerInfo[playerid][pLevel]);
					SCM(playerid,COLOR_GREY,"레벨 3이하이므로 자동으로 레벨업이 되었습니다.");
				}else{
				    SCM(playerid,COLOR_GREY,"당신의 경험치가 최대에 달하여 레벨업이 가능합니다.");
				}
			}
		}
	}
	return 1;
}

