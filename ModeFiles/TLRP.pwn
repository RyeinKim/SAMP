public Countdown()
{
	if(COunt > 0)
	{
		new string[128];
		if(COunt > 0)
		format(string, sizeof(string),"~w~COUNT~n~~w~- %d -",COunt);
		else
		format(string, sizeof(string),"~w~COUNT~n~~w~- START -");
		GameTextForAll(string,1000,3);
		SetTimer("Countdown",1000,0);
		COunt--;
		for(new i=0; i<M_P; i++)
		{
			if (IsPlayerConnected(i))
			{
				PlayerPlaySound(i,1056,-2982.8105,472.6976,4.9141);
			}
		}
		if(COunt < 0)
		{
			COunt = 0;
		}
	}
}
public OnPlayerTimer()
{
	for(new i=0; i<MAX_PLAYERS;i++)
	{
		if(WeaponHack == true)
		{
			if(IsPlayerConnected(i))
			{
			    if(PlayerLogin[i]==true)
			    {
				    switch(GetPlayerWeapon(i))
					{
						case 35,36,38,37,16,18,44,45,26,27:
						{
						    new string[256],year, month,day;
							getdate(year, month, day);
							format(string, sizeof(string), "%s님이 %s에 감지되었습니다.[/위성] (%d-%d-%d)", PlayerName(i), "서버에서 금지된 무기사용",year, month,day);
							SendAdminMessage(COLOR_WARNING,string,1);
						}
					}
				}
			}
		}
 	}
	return 1;
}
