		format(string,sizeof(string),"제한 이유: %s",banreason);
		SCM(playerid,COLOR_WHITE,string);
	    KickEx(playerid);
	    return 1;
	}
	format(string,sizeof(string),"IDBan/%s.txt",GetName(playerid));
	if(INI_Exist(string))
	{
		INI_Open(string,Read);
