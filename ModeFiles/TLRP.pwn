	        			ApplyAnimation(playerid,"BSKTBALL","BBALL_def_jump_shot",4.1,0,0,0,0,1);
						if(IsPlayerInRangeOfPoint(playerid, 2.0, Pos[0],Pos[1],Pos[2]-3 ))
						{
						    new String[128];
							format(String, sizeof(String), " %s이(가) 리바운드를 한다.", PlayerName(playerid));
