public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid)
{
	if(PlayerInfo[playerid][pUseAdmin]!=0)
	{
	    return 1;
	}
	new string[256];
	if(PlayerInfo[playerid][pDeathTime]!=0)
	{
	    SetPHealth(playerid,50);
	    return 1;
	}
	if(weaponid == TaserGun && GetPlayerState(playerid)==PLAYER_STATE_ONFOOT && GetPlayerState(issuerid)==PLAYER_STATE_ONFOOT)
	{
		new Float:x,Float:y,Float:z;
		GetPlayerPos(playerid,x,y,z);
		TogglePlayerControllable(playerid, 0);
		KillTimer(KillTimer5[playerid]);
		KillTimer5[playerid]=SetTimerEx("UnFreeze",20000,false,"i",playerid);
		if(IsPlayerInRangeOfPoint(issuerid,1,x,y,z))
		{
			format(string, sizeof(string), " %s이(가) %s에 의해 덮쳐져 제압을 당했습니다.", PlayerName(playerid) ,PlayerName(issuerid));
			BProxDetector(20.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
			Anim(playerid,"PED","KO_spin_R",4.1,0,1,1,1,1,1);
			return 1;
		}
		format(string, sizeof(string), " %s이(가) %s의 Taser X3 Gun에서 발사된 전기침에 맞아 쓰러진다.", PlayerName(playerid) ,PlayerName(issuerid));
		BProxDetector(20.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		Anim(playerid, "CRACK", "crckdeth4",4.1,0,1,1,1,1,1);
		return 1;
 	}
	if(weaponid != 0  && issuerid != INVALID_PLAYER_ID)
	{
		FoodEating[playerid]=0;
	}
	if(PlayerInfo[issuerid][pPackageUse] != 9999 && PlayerInfo[playerid][pPackageUse] == 9999 && PlayerInfo[playerid][pUseAdmin]==0 && issuerid != INVALID_PLAYER_ID )
	{
	    if(GetPlayerState(issuerid)!=PLAYER_STATE_DRIVER && GetPlayerState(playerid)==PLAYER_STATE_ONFOOT && PlayerInfo[playerid][pDeath]==0)
	    {
		    DamageWeapon[playerid]=0;
			Anim(playerid, "SWAT", "gnstwall_injurd",4.0, 0, 1, 1, 1, -1,1);
		    KillTimer(DamageWeaponTimer[playerid]);
		    DamageWeaponTimer[playerid]=SetTimerEx("NoWeaponHit",500,true,"i",playerid);
	    }
	}
	switch(weaponid)//무기대미지
	{
	    case 4: GivePlayerDamage(playerid, 20);
	    case 5: GivePlayerDamage(playerid, 10);
	    case 24: GivePlayerDamage(playerid, 50);
	    case 29: GivePlayerDamage(playerid, 20);
	    case 30: GivePlayerDamage(playerid, 20);
	    case 31: GivePlayerDamage(playerid, 25);
	    case 33: GivePlayerDamage(playerid, 75);
	    case 34: GivePlayerDamage(playerid, 100);
	    case 35: GivePlayerDamage(playerid, 1);
	    case 38: GivePlayerDamage(playerid, 1);
	}
 	new Float:armour, Float:health;
	GetPlayerArmour(playerid, armour);
	GetPlayerHealth(playerid, health);
	if(armour==0)
	{
	    if(health<=69)
	    {
	        new reason=weaponid;
			if(reason == 22)
			{ PlayerInfo[playerid][pDeathReason]=1; }
			else if(reason == 23)
