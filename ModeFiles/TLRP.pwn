public TAXIMITER(playerid)
{
	if(Taxi[playerid] == 1)
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			if(IsPlayerConnected(playerid))
			{
				new string[256],Float:distance;
				new vehicleid = GetPlayerVehicleID(playerid);
				distance=GetVehicleDistanceFromPoint(vehicleid,TaxiPos[playerid][0],TaxiPos[playerid][1],TaxiPos[playerid][2]);
				if(distance/50 >= 1)
				{
				    GetVehiclePos(vehicleid,TaxiPos[playerid][0],TaxiPos[playerid][1],TaxiPos[playerid][2]);
					TaxiCost[playerid] += TaxiPlus[playerid];
				}
				format(string, sizeof(string), "~n~~n~~n~~n~~n~~n~~n~~n~~y~Taxi IMITER ~r~~n~Cost: $%d", TaxiCost[playerid]);
				GameTextForPlayer(playerid, string, 2000,3);
				for(new i = 0; i < M_P; i++)
				{
					if(IsPlayerConnected(i))
					{
						if(vehicleid == GetPlayerVehicleID(i))
						{
							GameTextForPlayer(i, string, 2000,3);
						}
					}
				}
			}
		}
	}
	return 1;
}

