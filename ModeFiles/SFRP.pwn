public LoadFurniture()
{
	for(new idx=1;idx<MAX_FURNITURE;idx++)
	{
		new string[256];
        format(string,sizeof(string),"Furniture/%d.ini",idx);
        if(fexist(string))
        {
            FurnitureInfo[idx][mID] = idx;
            FurnitureInfo[idx][mModel] = dini_Int(string,"Model");
            FurnitureInfo[idx][mX] = dini_Float(string,"X");
            FurnitureInfo[idx][mY] = dini_Float(string,"Y");
            FurnitureInfo[idx][mZ] = dini_Float(string,"Z");
            FurnitureInfo[idx][mRX] = dini_Float(string,"RX");
            FurnitureInfo[idx][mRY] = dini_Float(string,"RY");
            FurnitureInfo[idx][mRZ] = dini_Float(string,"RZ");
            FurnitureInfo[idx][mvID] = dini_Int(string,"vID");
            FurnitureInfo[idx][mInt] = dini_Int(string,"Int");
            CreateFurniture(idx);
        }
	}
	return 1;
}

public CreateFurniture(ID)
{
	FurnitureInfo[ID][mObject] = CreateDynamicObject(FurnitureInfo[ID][mModel], FurnitureInfo[ID][mX], FurnitureInfo[ID][mY], FurnitureInfo[ID][mZ], FurnitureInfo[ID][mRX], FurnitureInfo[ID][mRY], FurnitureInfo[ID][mRZ], FurnitureInfo[ID][mvID], FurnitureInfo[ID][mInt]);
	new string[128];
	format(string, sizeof(string), "가구ID : %d\n오브젝트ID : %d\n", FurnitureInfo[ID][mID], FurnitureInfo[ID][mObject]);
	FurnitureInfo[ID][mText] = CreateDynamic3DTextLabel(string, 0xFFFFFFFF, FurnitureInfo[ID][mX], FurnitureInfo[ID][mY], FurnitureInfo[ID][mZ], 20.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, -1, FurnitureInfo[ID][mvID], FurnitureInfo[ID][mInt]);
    return 1;
}

public Lift(playerid)
{
    FurnitureInfo[PlayerUseingFurniture[playerid]][mLiftup] = true;
	PlayerLiftup[playerid] = true;
	ClearAnimations(playerid);
    ApplyAnimation(playerid, "CARRY", "crry_prtial", 4.0, 0, 0, 1, 1, 1);
	DestroyDynamic3DTextLabel(FurnitureInfo[PlayerUseingFurniture[playerid]][mText]);
    DestroyDynamicObject(FurnitureInfo[PlayerUseingFurniture[playerid]][mObject]);
	SetPlayerAttachedObject(playerid, 9, FurnitureInfo[PlayerUseingFurniture[playerid]][mModel], 1, 0, 0.6, 0, 0, 90, 0, 1, 1, 1);
    SCM(playerid,COLOR_YELLOW,"(>)"#C_WHITE" 가구를 들어 올렸습니다.");
	return 1;
}

public PutDown(playerid)
{
	PlayerPutDown[playerid] = false;
    FurnitureInfo[PlayerUseingFurniture[playerid]][mLiftup] = false;
    GetPlayerPos(playerid, FurnitureInfo[PlayerUseingFurniture[playerid]][mX], FurnitureInfo[PlayerUseingFurniture[playerid]][mY], FurnitureInfo[PlayerUseingFurniture[playerid]][mZ]);
    FurnitureInfo[PlayerUseingFurniture[playerid]][mZ] -= 0.5;
    FurnitureInfo[PlayerUseingFurniture[playerid]][mvID] = GetPlayerVirtualWorld(playerid);
    FurnitureInfo[PlayerUseingFurniture[playerid]][mInt] = GetPlayerInterior(playerid);
    CreateFurniture(PlayerUseingFurniture[playerid]);
    SCM(playerid,COLOR_YELLOW,"(>)"#C_WHITE" 가구를 내려 놓았습니다.");
    SaveFurniture(FurnitureInfo[PlayerUseingFurniture[playerid]][mID]);
    ApplyAnimation(playerid, "CARRY", "crry_prtial", 4.0, 0, 0, 0, 0, 0); //ClearAnimation
    rkrnsellect[playerid] = 0;
	return 1;
}
