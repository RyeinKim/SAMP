stock Call(playerid, phonenumb)
{
	new string[128], giveplayerid;
    format(string, sizeof(string), "* %s 이(가) 핸드폰을 꺼낸다.", PlayerNameEx(playerid));
    ApplyAnimation(playerid, "PED", "phone_in",FF[playerid],O1[playerid],O2[playerid],O3[playerid],O4[playerid],O5[playerid], 1);
	ProxDetector(5.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);

	if(phonenumb == 1655)
	{
		new mechanicworkers = 0;
		for(new i=0; i<M_P; i++)
		{
			if(IsPlayerConnected(i))
			{
				if(PlayerInfo[i][pJob] == 4)
				{
					mechanicworkers++;
				}
			}
		}
		if(mechanicworkers < 1)
		{
			SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 현재 업무중인 정비사가 없습니다.");
			return 1;
		}

		format(string, sizeof(string), "* %s 이(가) 정비사를 호출합니다. (/동의 하이카)", PlayerNameEx(playerid));
		SendJobMessage(4, COLOR_YELLOW, string);
		SCM(playerid, COLOR_YELLOW, "( 문자 ) - 하이카 센터를 이용해주셔서 감사합니다. ( 전화번호 1655 )");
		MechanicCall = playerid;
		return 1;
	}

	if(phonenumb == 112)
	{
		SCM(playerid, COLOR_YELLOW, "( 통화 ) - 112 센터입니다. 무엇을 도와 드릴까요? ( 전화번호 112 )");
		Mobile[playerid] = 112;
		return 1;
	}
	if(phonenumb == 119)
	{
		SCM(playerid, COLOR_YELLOW, "( 통화 ) - 119 센터입니다. 무엇을 도와 드릴까요? ( 전화번호 119 )");
		Mobile[playerid] = 119;
		return 1;
	}
	if(phonenumb == PlayerInfo[playerid][pPnumber])
	{
		SCM(playerid, COLOR_YELLOW, "( 통화 ) - 이번호는 고객님의 번호입니다.");
		return 1;
	}
	if(Mobile[playerid] != 999)
	{
		SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 당신은 이미 통화를 하고 있습니다.");
		return 1;
	}
	for(new i = 0; i < M_P; i++)
	{
		if(IsPlayerConnected(i))
		{
			if(PlayerInfo[i][pPnumber] == phonenumb && phonenumb != 0)
			{
				giveplayerid = i;
				if(IsPlayerConnected(giveplayerid))
				{
					if(giveplayerid != INVALID_PLAYER_ID)
					{
						if(PlayerInfo[giveplayerid][pPonline] > 0)
						{
       						SCM(playerid, COLOR_YELLOW, "( 통화 ) - 고객님의 전화기가 꺼져있습니다.");
							return 1;
						}
						if(RingTone[giveplayerid] || Mobile[giveplayerid] != 999)
						{
       						SCM(playerid, COLOR_YELLOW, "( 통화 ) - 지금 거신 고객님은 통화중입니다.");
							return 1;
						}
						if(Mobile[giveplayerid] == 999)
						{
						    Mobile[playerid] = giveplayerid;
							format(string, sizeof(string), "휴대폰에서 벨소리가 울립니다. ( 전화번호 %d )", PlayerInfo[playerid][pPnumber]);
							SCM(giveplayerid, COLOR_YELLOW, string);
							RingTone[giveplayerid] = 10;
							format(string, sizeof(string), "* %s 의 휴대폰에서 벨소리가 울린다.", PlayerNameEx(giveplayerid));
							ProxDetector(5.0, i, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
							SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 당신은 휴대폰으로 대화중입니다. (/끊기)를 이용해 끊을 수 있습니다.");
							CellTime[playerid] = 1;
							return 1;
						}
					}
				}
			}
		}
	}
	SCM(playerid, COLOR_YELLOW, "( 통화 ) - 지금 거신 번호는 없는 번호입니다.");
	return 1;
}

