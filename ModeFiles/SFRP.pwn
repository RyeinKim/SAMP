public OnVehicleUpdate(vehicleid)
{
	if(GetVehicleModel(vehicleid)==0)
	{
	    OnVehicleDestroy(vehicleid);
	    KillTimer(UpdateVehicleTimer[vehicleid]);
	    return 1;
	}
	new Float:vhealth;
	GetVehicleHealth(vehicleid, vhealth);
	new engine,lights,alarm,doors,bonnet,boot,objective;
	GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
	if(CarInfo[vehicleid][cLock] == 1 && doors != 1)
	{
		SetVehicleParamsEx(vehicleid,engine,lights,alarm,1,bonnet,boot,objective);
	}
	else if(CarInfo[vehicleid][cLock] == 0 && doors == 1)
	{
		SetVehicleParamsEx(vehicleid,engine,lights,alarm,0,bonnet,boot,objective);
	}
	if(vhealth > CarInfo[vehicleid][cRealHealth]&&CarInfo[vehicleid][cCreate]==1)
	{
		SetVehicleHealth(vehicleid,CarInfo[vehicleid][cRealHealth]);
	}
	if(vhealth <= 300)
	{
		if(GetVehicleVirtualWorld(vehicleid) < 900)
		{
			SetVehicleParamsEx(vehicleid,0,0,alarm,doors,bonnet,boot,objective);
			SetVehicleHealth(vehicleid, 300);
			if(CarInfo[vehicleid][CheckRepair]==0&&CarInfo[vehicleid][cMember]==0&&CarInfo[vehicleid][cCreate]==1)
			{
			    CarInfo[vehicleid][CheckRepair]=1;
			    for(new playerid=0;playerid<M_P;playerid++)
			    {
			        if(IsPlayerConnected(playerid))
			        {
			            if(IsPlayerInVehicle(playerid,vehicleid))
			            {
							if(IsPlayerCar(playerid, vehicleid))
							{
							    new Float:angle_Z;
								GetVehicleZAngle(vehicleid, angle_Z);
								SetVehicleZAngle(vehicleid, floatround(angle_Z, floatround_round));
								CarInfo[vehicleid][cRealHealth]=floatsub(CarInfo[vehicleid][cRealHealth],float(random(11)+20));
								SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 차량의 내구도가 감소하였습니다.");
								break;
							}
						}
					}
				}
			    if(CarInfo[vehicleid][cRealHealth]<=349)
				{
				    for(new playerid=0;playerid<M_P;playerid++)
				    {
				        if(IsPlayerConnected(playerid))
				        {
				            if(IsPlayerInVehicle(playerid,vehicleid))
				            {
								if(IsPlayerCar(playerid, vehicleid))
								{
									for(new hc = 0; hc < MAX_HAVE_CAR; hc++)
									{
										if(strcmp(PlayerCarKey[playerid][hc], CarInfo[vehicleid][cLP], true) == 0)
										{
											strmid(PlayerCarKey[playerid][hc], "None", 0, strlen("None"), 255);
											break;
										}
									}
									new file[256];
									format(file, sizeof(file), "Cars/%s.ini", CarInfo[vehicleid][cLP]);
									dini_Remove(file);
									PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
									ResetCarInfo(vehicleid);
									DestroyVehicle(vehicleid);
									SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 차량을 더 이상 사용할수가 없어 폐차되었습니다.");
									break;
								}
							}
						}
					}
				}
			}
		}
	}
	else if(vhealth >= 700)
	{
		if(IsACop2(2, CarInfo[vehicleid][cMember]) || CarInfo[vehicleid][cMember] == 4)
		{
			if(vhealth < 2000)
			{
				new panels, doors2, lights2, tires;
				GetVehicleDamageStatus(vehicleid, panels, doors2, lights2, tires);
				if(panels != 0 || doors != 0 || lights != 0)
				{
					UpdateVehicleDamageStatus(vehicleid, 0, 0, 0, tires);
					SetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
				}
			}
		}
		else
		{
			if(vhealth < 1000)
			{
				new panels, doors2, lights2, tires;
				GetVehicleDamageStatus(vehicleid, panels, doors2, lights2, tires);
				if(panels != 0 || doors != 0 || lights != 0)
				{
					UpdateVehicleDamageStatus(vehicleid, 0, 0, 0, tires);
					SetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
				}
			}
		}
	}
	GetVehicleHealth(vehicleid, vhealth);
    if(CarInfo[vehicleid][cCreate]==1)
    {
		if(CarInfo[vehicleid][cHealth]<vhealth)
		{
			SetVehicleHealth(vehicleid, CarInfo[vehicleid][cHealth]);
			if(CarInfo[vehicleid][cHealth]<=300)
			{
			    CarInfo[vehicleid][cHealth]=300;
			    new Float:angle_Z;
				GetVehicleZAngle(vehicleid, angle_Z);
				SetVehicleZAngle(vehicleid, floatround(angle_Z, floatround_round));
			}
		}else{
			GetVehicleHealth(vehicleid, CarInfo[vehicleid][cHealth]);
		}
	}
	for(new i = 0; i < M_P; i++)
	{
		if(GetPlayerVehicleID(i) == vehicleid)
		{
			GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
		 	if(engine != 1 )
		 	{

				new Float:sp[3];
		 		GetVehicleVelocity(vehicleid, sp[0], sp[1], sp[2]);
		 		if(StopVehicle[vehicleid])
		 			SetVehicleVelocity(vehicleid, 0, 0, sp[2]);
		 		else
		 			SetVehicleVelocity(vehicleid, sp[0]-(sp[0]/10), sp[1]-(sp[1]/10), sp[2]);

		 		if(sp[0] == 0 && sp[1] == 0)
					StopVehicle[vehicleid] = true;
		 	}
		 	else
		 	{
		 		StopVehicle[vehicleid] = false;
		 	}
	 	}
 	}

	if(!IsPlayerConnected(RappelVehicle[vehicleid]))
	{
	    RappelVehicle[vehicleid]=999;
	    DestroyObject(RappelObject[vehicleid]);
	}

 	if(RappelVehicle[vehicleid]!=999)
 	{

	 	SetVehicleVelocity(vehicleid,0,0,0);
 	}

	new item = GetVehicleComponentInSlot(vehicleid, 5);
	if(item == 1010 && Nitroused[vehicleid]==false)
	{
		Nitroused[vehicleid]=true;
		NitroPower[vehicleid]=100;
	}else if(item == 1009 && Nitroused[vehicleid]==true)
	{
		Nitroused[vehicleid]=false;
		NitroPower[vehicleid]=0;
	}else if(item == 1008 && Nitroused[vehicleid]==true)
	{
		Nitroused[vehicleid]=false;
		NitroPower[vehicleid]=0;
	}
	return 1;
}
