public SyncTime()
{



	new tmphour;
	new tmpminute;
	new tmpsecond;
	new y,m,d;
	new data[24];
	gettime(tmphour, tmpminute, tmpsecond);
	getdate(y, m, d);
	if((tmphour > ghour) || (tmphour == 0 && ghour == 23))
	{
	    new string[128];
		ghour = tmphour;
		passwordgate=random(89999)+10000;
		format(string, sizeof(string), " * Console password is [ %d ]",passwordgate);
		SendFamilyMessage(8, COLOR_RADIO, string);
		if(realtime)
		{
			SetWorldTime(tmphour+3);
		}
	}
	format(data, sizeof(data), "%d|%d|%d", y, m ,d);
	if(strcmp(data, GameInfo[gTime], true) == 1)
	{
		for(new b = 0; b < sizeof(BizzInfo); b++)
		{
			if(BizzInfo[b][bCreate] == 1)
			{
				if(BizzInfo[b][bBills] == 0)
				{
					BizzInfo[b][bBills] = (LoadInteriorprice(BizzInfo[b][bInterior]) + BizzInfo[b][bPrice])/500;
				}
			}
		}
		for(new h = 0; h < sizeof(HouseInfo); h++)
		{
			if(HouseInfo[h][hCreate] == 1)
			{
				if(HouseInfo[h][hBills] == 0)
				{
					HouseInfo[h][hBills] = (LoadInteriorprice(HouseInfo[h][hInterior]) + HouseInfo[h][hPrice])/500;
				}
			}
		}
	}
	strmid(GameInfo[gTime], data, 0, strlen(data), 255);
	for(new v = 1; v < MAX_VEHICLES; v++)
	{
		new engine,lights,alarm,doors,bonnet,boot,objective;
		GetVehicleParamsEx(v,engine,lights,alarm,doors,bonnet,boot,objective);
		if(engine == 1)
		{
			if(!IsABike(v))
				CarInfo[v][cGas] --;
		}
		if(CarInfo[v][cGas] <= 0)
		{
			if(engine == 1)
			{
				SetVehicleParamsEx(v,0,0,alarm,doors,bonnet,boot,objective);
			}
		}
	}
	return 1;
}

public GetClosestPlayer(p1)
{
	new x,Float:dis,Float:dis2,player;
	player = -1;
	dis = 99999.99;
	for (x=0;x<M_P;x++)
	{
		if(IsPlayerConnected(x))
		{
			if(x != p1)
			{
				dis2 = GetDistanceBetweenPlayers(x,p1);
				if(dis2 < dis && dis2 != -1.00)
				{
					dis = dis2;
					player = x;
				}
			}
		}
	}
	return player;
}

public PayDay(playerid)
{
	new sumpay[4];
	new string[256];
	new interest,tmpintrate=1;

	SCM(playerid,COLOR_BLUE,"__________페이__________");
	if(PlayerInfo[playerid][pJob]!=0)
	{
	    sumpay[0]=200;
	}
	else if(PlayerInfo[playerid][pLeader] == 1 && PlayerInfo[playerid][pMember] == 1 || PlayerInfo[playerid][pLeader] == 2  && PlayerInfo[playerid][pMember] == 2 || PlayerInfo[playerid][pLeader] == 4  && PlayerInfo[playerid][pMember] == 4 || PlayerInfo[playerid][pLeader] == 9 && PlayerInfo[playerid][pMember] == 9)
	{
	    new rksqn = GetVarInt("정부간부급시급");
	    sumpay[0]=rksqn;
		format(string, sizeof(string), "정부군 간부 급여 %d 원",rksqn);
		SCM(playerid, COLOR_ADMIN, string);
	}
	else if(PlayerInfo[playerid][pLeader] == 0 && PlayerInfo[playerid][pMember] == 1 || PlayerInfo[playerid][pLeader] == 0  && PlayerInfo[playerid][pMember] == 2 || PlayerInfo[playerid][pLeader] == 0  && PlayerInfo[playerid][pMember] == 4 || PlayerInfo[playerid][pLeader] == 0 && PlayerInfo[playerid][pMember] == 9)
	{
		new dlfqks = GetVarInt("정부일반시급");
	    sumpay[0]=dlfqks;
		format(string, sizeof(string), "정부군 일반 급여 %d 원",dlfqks);
		SCM(playerid, COLOR_ADMIN, string);
	}
	sumpay[0] += PlayerInfo[playerid][pLevel];
	if(PlayerInfo[playerid][pJob] != 0 && PlayerInfo[playerid][pLevel] <= 30)
	{
	    new tlals = GetVarInt("시민직시급");
		sumpay[0] += tlals;
 		format(string, sizeof(string), "시민직 지원금 %d 원",sumpay[0]);
		SCM(playerid, COLOR_ADMIN, string);
	}
	for(new hc = 0; hc < MAX_HAVE_CAR; hc++)
	{
		if(strcmp(PlayerCarKey[playerid][hc], "None", true) == 1)
		{
			if(CheckCars(PlayerCarKey[playerid][hc]))
			{
			    new vmodel;
				format(string, sizeof(string), "Cars/%s.ini", PlayerCarKey[playerid][hc]);
		        if(INI_Exist(string))
				{
				    INI_Open(string,Read);
				    vmodel=strval(INI_Read("C_Model"));
				    INI_Close();
				}
				new cp = floatround(float(LoadCarprice(vmodel))*0.001, floatround_round);
				sumpay[1] += cp;
				if(cp == 0) sumpay[1] += random(50);
			}
		}
	}
	sumpay[0] -= sumpay[1];
	for(new h = 0; h < MAX_HOUSES; h++)
	{
		if(strcmp(GetName(playerid), HouseInfo[h][hOwner], true) == 0 && HouseInfo[h][hCreate]==1 && HouseInfo[h][hOwned]==1)
		{
		    sumpay[2] += floatround((float(LoadInteriorprice(HouseInfo[h][hInterior])+HouseInfo[h][hPrice]))*0.001, floatround_round);
		    if(sumpay[2] == 0)
		        sumpay[2] = random(250);
			sumpay[0] -= sumpay[2];
		}
		if(PlayerInfo[playerid][pPhousekey]==h && strcmp(GetName(playerid), HouseInfo[h][hOwner], true) != 0 && HouseInfo[h][hCreate]==1)
		{
//				PlayerInfo[playerid][pPhousekey] = h;
			SafeGivePlayerMoney(playerid,-HouseInfo[h][hRent]);
			HouseInfo[h][hTakings] += HouseInfo[h][hRent];
		}else if(PlayerInfo[playerid][pPhousekey]==h && HouseInfo[h][hCreate]==0)
		{
		    PlayerInfo[playerid][pPhousekey]=999;
		}
	}

	for(new b = 0; b < MAX_HOUSES; b++)
	{
		if(strcmp(GetName(playerid), BizzInfo[b][bOwner], true) == 0 && BizzInfo[b][bCreate]==1 && BizzInfo[b][bOwned]==1)
		{
		    sumpay[3] += floatround((float(LoadInteriorprice(BizzInfo[b][bInterior])+BizzInfo[b][bPrice]))*0.001, floatround_round);
		    if(sumpay[3] == 0)
		        sumpay[3] = random(300);
			sumpay[0] -= sumpay[3];
		}
	}

	SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
	
	
	format(string, sizeof(string), "자동차 세금 ( - %d 원 )", sumpay[1]);
	SCM(playerid,COLOR_ADMIN, string);
	format(string, sizeof(string), "집 월세      ( - %d 원 )", sumpay[2]);
	SCM(playerid,COLOR_ADMIN, string);
	format(string, sizeof(string), "사업장 월세 ( - %d 원 )", sumpay[3]);
	SCM(playerid,COLOR_ADMIN, string);
	interest=(PlayerInfo[playerid][pAccount]/1000)*(tmpintrate);
	format(string, sizeof(string), "이자 소득    ( + %d 원 )", interest);
	SCM(playerid,COLOR_ADMIN, string);
