public CheckForWalkingTeleport(playerid) //입장퇴장
{
	for(new i = 0; i < MAX_POS; i++)
	{
		if(Pos[i][enterjob] == 0 && PlayerToPoint(2, playerid,Pos[i][enterposx], Pos[i][enterposy], Pos[i][enterposz])  || Pos[i][enterjob] != 0 && Pos[i][enterjob] == PlayerInfo[playerid][pMember] && PlayerToPoint(1, playerid,Pos[i][enterposx], Pos[i][enterposy], Pos[i][enterposz]))
		{
			if(Pos[i][enterposvws] == GetPlayerVirtualWorld(playerid))
			{
				if(Pos[i][Usetype] != 0)
				{
					SetPlayerVirtualWorld(playerid,Pos[i][exitposvws]);
					SafeSetPlayerPos(playerid, Pos[i][exitposx],Pos[i][exitposy],Pos[i][exitposz]);
					SetPlayerInterior(playerid,Pos[i][exitposints]);
					FreezeEnter(playerid);
					return 1;
				}
			}
		}
		else if(Pos[i][exitjob] == 0 && PlayerToPoint(2, playerid,Pos[i][exitposx], Pos[i][exitposy], Pos[i][exitposz]) || Pos[i][exitjob] != 0 && Pos[i][exitjob] == PlayerInfo[playerid][pMember] && PlayerToPoint(1, playerid,Pos[i][exitposx], Pos[i][exitposy], Pos[i][exitposz]))
		{
			if(Pos[i][exitposvws] == GetPlayerVirtualWorld(playerid))
			{
				if(Pos[i][Usetype] != 0)
				{
					SetPlayerVirtualWorld(playerid,Pos[i][enterposvws]);
					SafeSetPlayerPos(playerid, Pos[i][enterposx],Pos[i][enterposy],Pos[i][enterposz]);
					SetPlayerInterior(playerid,Pos[i][enterposints]);
					FreezeEnter(playerid);
					return 1;
				}
			}
		}
	}
	for(new h = 0; h < MAX_HOUSES; h++)
	{
		if(PlayerToPoint(2, playerid,HouseInfo[h][hEntrancex], HouseInfo[h][hEntrancey], HouseInfo[h][hEntrancez]) && HouseInfo[h][hLock] == 0 && GetPlayerVirtualWorld(playerid) == HouseInfo[h][hEworld])
		{
			if(HouseInfo[h][hCreate] == 1)
			{
				SetPlayerInterior(playerid,InteriorInfo[HouseInfo[h][hInterior]][iInterior]);
				SetPlayerVirtualWorld(playerid,h+100);
				SafeSetPlayerPos(playerid, InteriorInfo[HouseInfo[h][hInterior]][iX], InteriorInfo[HouseInfo[h][hInterior]][iY], InteriorInfo[HouseInfo[h][hInterior]][iZ]);
				FreezeEnter(playerid);
				return 1;
			}
		}
		else if(PlayerToPoint(2, playerid,InteriorInfo[HouseInfo[h][hInterior]][iX], InteriorInfo[HouseInfo[h][hInterior]][iY], InteriorInfo[HouseInfo[h][hInterior]][iZ]) && GetPlayerVirtualWorld(playerid) == h+100)
		{
			if(HouseInfo[h][hCreate] == 1)
			{
				SetPlayerInterior(playerid,HouseInfo[h][hEint]);
				SetPlayerVirtualWorld(playerid,HouseInfo[h][hEworld]);
				SafeSetPlayerPos(playerid, HouseInfo[h][hEntrancex], HouseInfo[h][hEntrancey], HouseInfo[h][hEntrancez]);
				FreezeEnter(playerid);
				return 1;
			}
		}
		else if(PlayerToPoint(2, playerid,BizzInfo[h][bEntrancex], BizzInfo[h][bEntrancey], BizzInfo[h][bEntrancez]) && BizzInfo[h][bLock] == 0 && GetPlayerVirtualWorld(playerid) == BizzInfo[h][bEworld])
		{
			if(BizzInfo[h][bCreate] == 1)
			{
				if(SafeGetPlayerMoney(playerid) < BizzInfo[h][bCost])
				{
					SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 돈이 부족 합니다.");
					return 1;
				}
				SetPlayerInterior(playerid,InteriorInfo[BizzInfo[h][bInterior]][iInterior]);
				SetPlayerVirtualWorld(playerid,h+MAX_HOUSES+100);
				SafeSetPlayerPos(playerid, InteriorInfo[BizzInfo[h][bInterior]][iX], InteriorInfo[BizzInfo[h][bInterior]][iY], InteriorInfo[BizzInfo[h][bInterior]][iZ]);
				if(BizzInfo[h][bOwned] == 1)
				{
					SafeGivePlayerMoney(playerid, -BizzInfo[h][bCost]);
					BizzInfo[h][bTakings] += BizzInfo[h][bCost];
				}
				return 1;
			}
		}
		else if(PlayerToPoint(2, playerid, InteriorInfo[BizzInfo[h][bInterior]][iX], InteriorInfo[BizzInfo[h][bInterior]][iY], InteriorInfo[BizzInfo[h][bInterior]][iZ]) && GetPlayerVirtualWorld(playerid) == h+MAX_HOUSES+100)
		{
			if(BizzInfo[h][bCreate] == 1)
			{
				SetPlayerVirtualWorld(playerid,BizzInfo[h][bEworld]);
				SetPlayerInterior(playerid,BizzInfo[h][bEint]);
				SafeSetPlayerPos(playerid, BizzInfo[h][bEntrancex], BizzInfo[h][bEntrancey], BizzInfo[h][bEntrancez]);
				return 1;
			}
		}
	}
	PlayerEnterCarPark(playerid);
	return 1;
}

