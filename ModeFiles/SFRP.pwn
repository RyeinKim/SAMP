public BackupClear(playerid, calledbytimer)
{
	if(IsPlayerConnected(playerid))
	{
		if(IsACop(2, playerid))
		{
			if(PlayerInfo[playerid][pRequestingBackup] == 1)
			{
				for(new i = 0; i < M_P; i++)
				{
					if(IsPlayerConnected(i))
					{
						if(IsACop(2, i))
						{
							SetPlayerMarkerForPlayer(i, playerid, TEAM_HIT_COLOR);
						}
					}
				}
				if(calledbytimer != 1)
				{
					SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 지원요청이 취소되었습니다.");
				}
				else
				{
					SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 당신의 지원요청이 자동적으로 취소되었습니다.");
				}
				SetPlayerColor(playerid,DutyColor[playerid]);
				DutyColor[playerid]=TEAM_HIT_COLOR;
				PlayerInfo[playerid][pRequestingBackup] = 0;
			}
			else
			{
				if(calledbytimer != 1)
				{
					SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 당신은 지원요청을 하지 않았습니다.");
				}
			}
		}
		else
		{
			if(calledbytimer != 1)
			{
				SCM(playerid, COLOR_YELLOW, "(>)"#C_WHITE" 당신은 이 명령어를 사용할 권한이 없습니다.");
			}
		}
	}
	return 1;
}

