	format(string,sizeof(string),"가구ID : %d\n오브젝트ID : %d\n\n", FurnitureInfo[PlayerUseingFurniture[playerid]][mID], FurnitureInfo[PlayerUseingFurniture[playerid]][mObject]);
	if(response == EDIT_RESPONSE_FINAL)
	{
		FurnitureInfo[PlayerUseingFurniture[playerid]][mX] = x;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mY] = y;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mZ] = z;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mRX] = rx;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mRY] = ry;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mRZ] = rz;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mvID] = GetPlayerVirtualWorld(playerid);
		FurnitureInfo[PlayerUseingFurniture[playerid]][mInt] = GetPlayerInterior(playerid);
		SetDynamicObjectPos(objectid, x, y, z);
		SetDynamicObjectRot(objectid, rx, ry, rz);
		FurnitureInfo[PlayerUseingFurniture[playerid]][mText] = CreateDynamic3DTextLabel(string, 0xFFFFFFFF, FurnitureInfo[PlayerUseingFurniture[playerid]][mX], FurnitureInfo[PlayerUseingFurniture[playerid]][mY], FurnitureInfo[PlayerUseingFurniture[playerid]][mZ], 20.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, -1, FurnitureInfo[PlayerUseingFurniture[playerid]][mvID], FurnitureInfo[PlayerUseingFurniture[playerid]][mInt]);
		SaveFurniture(FurnitureInfo[PlayerUseingFurniture[playerid]][mID]);
	}
	if(response == EDIT_RESPONSE_CANCEL)
	{
		SetDynamicObjectPos(objectid, oldX, oldY, oldZ);
		SetDynamicObjectRot(objectid, oldRotX, oldRotY, oldRotZ);
		FurnitureInfo[PlayerUseingFurniture[playerid]][mX] = oldX;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mY] = oldY;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mZ] = oldZ;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mRX] = oldRotX;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mRY] = oldRotY;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mRZ] = oldRotZ;
		FurnitureInfo[PlayerUseingFurniture[playerid]][mvID] = GetPlayerVirtualWorld(playerid);
		FurnitureInfo[PlayerUseingFurniture[playerid]][mInt] = GetPlayerInterior(playerid);
		FurnitureInfo[PlayerUseingFurniture[playerid]][mText] = CreateDynamic3DTextLabel(string, 0xFFFFFFFF, FurnitureInfo[PlayerUseingFurniture[playerid]][mX], FurnitureInfo[PlayerUseingFurniture[playerid]][mY], FurnitureInfo[PlayerUseingFurniture[playerid]][mZ], 20.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, -1, FurnitureInfo[PlayerUseingFurniture[playerid]][mvID], FurnitureInfo[PlayerUseingFurniture[playerid]][mInt]);
	}
	SaveFurniture(FurnitureInfo[PlayerUseingFurniture[playerid]][mID]);
	FurnitureInfo[PlayerUseingFurniture[playerid]][mLiftup] = false;
}

