forward ServerSendCommand(g2mx);
public ServerSendCommand(g2mx)
{
	new year, month,day,hour,minute,second,string[128];
	getdate(year, month, day);
	gettime(hour,minute,second);
	format(string,sizeof(string),"%d년 %d월 %d일 %d시 %d분 %d초",year,month,day,hour,minute,second);
	for(new i = 0; i < M_P; i++)
	{
		strmid(PlayerInfo[i][pLastConnect],string,0,strlen(string),256);
		OnPlayerSaveInfo(i);
		OnPlayerUpdateEx(i);
		TogglePlayerControllable(i, 0);
//		Kick(i);
	}
	for(new c = 1; c <MAX_VEHICLES; c++)
	{
		if(CarInfo[c][cCreate] != 0)
		{
			SaveCar(c);
		}
	}
	for(new b = 0; b < MAX_HOUSES; b++)
	{
		if(BizzInfo[b][bCreate] != 0)
		{
			SaveBizz(b);
		}
	}
	for(new h = 0; h < MAX_HOUSES; h++)
	{
		if(HouseInfo[h][hCreate] != 0)
		{
			SaveHouse(h);
		}
	}
	for(new f = 1; f < MAX_FACTIONS; f++)
	{
		if(FactionInfo[f][fCreate] != 0)
		{
			SaveFaction(f);
		}
	}

	for(new cp = 0; cp < MAX_CARPARK; cp++)
	{
		if(CarParkInfo[cp][cpCreate] != 0)
		{
			SaveCarPark(cp);
		}
	}
	for(new p = 0; p < MAX_POS; p++)
	{
		if(Pos[p][Usetype] != 0)
		{
			SavePos(p);
		}
	}
	for(new z = 0; z < sizeof(ZoneInfo); z++)
	{
		SaveZone(z);
	}
	SaveGameInfo();
	for(new s = 0; s < MAX_SPRAYS; s++)
	{
		SaveSprayTags(s);
	}
	for(new t = 0; t < sizeof(Timer); t++)
	{
		KillTimer(Timer[t]);
	}
	SetTimerEx("ServerCommand",5000,false,"i",g2mx);
	return 1;
}

