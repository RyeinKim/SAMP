stock ShowPlayerPhonebook(playerid, dialogid, caption[], button1[], button2[], mode=0)
{
    new dialogstring[1024], string[128];
    new b = 1;
    if(mode == 1) strmid(dialogstring, ""#C_BLUE"직접 입력\n", 0, 1024, 1024);
    else if(mode == 2) strmid(dialogstring, ""#C_BLUE"추가\n", 0, 1024, 1024);
    else b = 0;
	for(new a; a < MAX_PBSLOTS; a++)
	{
	    format(string, sizeof string, "Phonebook%d", b);
	    DeletePVar(playerid, string);
		if(strlen(PhonebookInfo[playerid][a][pbName]))
		{
		    SetPVarInt(playerid, string, a);
		    b++;
		    format(dialogstring, sizeof dialogstring, "%s%s\n", dialogstring, PhonebookInfo[playerid][a][pbName]);
		}
	}
	if(!b && !mode)
		return ShowPlayerDialog(playerid, 0, DIALOG_STYLE_MSGBOX, "전화번호부", ""#C_BLUE"목록이 비어 있습니다.", "확인", "");
	return ShowPlayerDialog(playerid, dialogid, DIALOG_STYLE_LIST, caption, dialogstring, button1, button2);
}

