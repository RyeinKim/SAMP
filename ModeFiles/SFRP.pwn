stock SetVarString(const Name[], var[])
{
	new b = INVALID_VAR_ID;
	if (!strlen(Name)) return b;
	for (new a = 0; a < MAX_VARS; a++)
	{
	    if (!strcmp(VarName[a], Name, true) && strlen(VarName[a]))
	    {
			SetGVarString(Name, var);
			SaveVar(a);
			return a;
		}
		else if (!strlen(VarName[a])) b = a;
	}
	if (b == INVALID_VAR_ID) return b;
	strmid(VarName[b], Name, 0, 256, 256);
	SetGVarString(Name, var);
	SaveVar(b);
	return b;
}
RespawnVehicle()
{
	new bool:used[MAX_VEHICLES] = { false, ... };
	for(new p; p<M_P; p++)
	{
		if(IsPlayerInAnyVehicle(p))
		{
			new veh = GetPlayerVehicleID(p);
			used[veh] = true;
		}
	}
	for(new v; v<MAX_VEHICLES; v++)
	{
		if(!used[v])
			{
				SetVehicleToRespawn(v);
    			SetTimerEx("SetVehicleZAnglePos", 1000, false, "i", v);
			}
  	}

}
