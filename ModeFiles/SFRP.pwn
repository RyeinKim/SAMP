public OnPlayerDisconnect(playerid, reason)
{
    if(PlayerLiftup[playerid] == true)
	{
	    PlayerLiftup[playerid] = false;
    	FurnitureInfo[PlayerUseingFurniture[playerid]][mLiftup] = false;
    	CreateFurniture(PlayerUseingFurniture[playerid]);
    	RemovePlayerAttachedObject(playerid, 9);
	}
	if(PlayerDrivingTest[playerid] != 0)
	{
		dnswjstlgjawnd = 0;
	}
	if (PlayerVelocimetro[playerid])
	{
	    KillTimer(PlayerVelocimetroTimer[playerid]);
	}


	SetCameraBehindPlayer(playerid);
	TogglePlayerControllable(playerid, 1);

	new string[128];
	new Reason[256];
	new year, month,day,hour,minute,second;

	getdate(year, month, day);
	gettime(hour,minute,second);
    OnPlayerUpdateEx(playerid);

	if(gmx==0)
	{
		format(string,sizeof(string),"%d년 %d월 %d일 %d시 %d분 %d초",year,month,day,hour,minute,second);
		strmid(PlayerInfo[playerid][pLastConnect],string,0,strlen(string),256);
	}
	switch(reason)
	{
		case 0: Reason = "비 정상종료";
		case 1: Reason = "정상종료";
		case 2: Reason = "관리자 권한 종료";
	}
	format(string, sizeof(string), "%s - (%s)", PlayerNameEx(playerid), Reason);
	ProxDetector(30.0, playerid, string, COLOR_FADE1, COLOR_FADE2, COLOR_FADE3, COLOR_FADE4, COLOR_FADE5);

	if(gmx != 1)
	{
		format(string, sizeof(string), "(>)"#C_WHITE" %s(%d)님 접속종료 [ 사유 : %s ]", PlayerNameEx(playerid), playerid, Reason);
		SAM(COLOR_YELLOW, string);
	}
	if(PlayerDrivingTest[playerid] != 0)
	{
		DestroyVehicle(PlayerDrivingTest[playerid]);
	}
	if(HireCar[playerid] != 0)
	{
		CarInfo[gLastCar[playerid]][cGas] = 100;
		Ttime[playerid] = 0;
		if(HireCarType[playerid] == 1)
			DestroyVehicle(HireCar[playerid]);
		else if(HireCarType[playerid] == 2)
			SetVehicleToRespawn(HireCar[playerid]);
		HireCar[playerid] = 0;
		HireCarType[playerid] = 0;
	}
	for(new a = 0; a < 5; a++)
		DestroyDynamicObject(RoadBlock[playerid][a]);
	if(Draging[playerid] != 999)
		Draging[Draging[playerid]] = 999;
	if(ChatLog[playerid] != 999)
	{
		ChatLog[playerid] = 999;
	}
	for(new i = 0; i < M_P; i++)
	{
		if(IsPlayerConnected(i))
		{
			if(Mechanic[i] != 999)
			{
				if(Mechanic[i] == playerid)
				{
					Mechanic[i] = 999;
					GameTextForPlayer(i, "~w~Mechanic Caller~n~~r~Left the game", 5000, 1);
					SCM(playerid,COLOR_YELLOW, "( 문자 ) - 정비 요청자가 요청을 취소 했습니다. ( 사유 : 접속종료 )");
					NomalCheckpoint[i] = false;
					DisablePlayerCheckpoint(i);
				}
			}
		}
	}
	if(PlayerInfo[playerid][pAdmin] > 0)
	{
		SetPlayerName(playerid, GetName(playerid));
	}
	if(gmx==0)
	{
		if(PlayerInfo[playerid][pAdminMode] == 2)
		{
		    PlayerInfo[playerid][pAdminMode] = 1;
		}
		OnPlayerUpdateEx(playerid);
	}
	ResetPlayerVariable(playerid);
    OnPlayerUpdateEx(playerid);
	return 1;
}

