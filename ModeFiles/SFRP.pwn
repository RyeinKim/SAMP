public SaveBizz(h)
{
	if(BizzInfo[h][bCreate])
	{
 		new dir[256];
		format(dir,sizeof(dir),"Bizzs/%d.ini",h);
		INI_Open(dir,Write);
    	INI_WriteInt("B_Create",BizzInfo[h][bCreate]);
    	INI_WriteFloat("B_X",BizzInfo[h][bEntrancex]);
    	INI_WriteFloat("B_Y",BizzInfo[h][bEntrancey]);
    	INI_WriteFloat("B_Z",BizzInfo[h][bEntrancez]);
    	INI_WriteInt("B_Interior",BizzInfo[h][bInterior]);
    	INI_WriteInt("B_Eint",BizzInfo[h][bEint]);
    	INI_WriteInt("B_Eworld",BizzInfo[h][bEworld]);
    	INI_Write("B_Name",BizzInfo[h][bName]);
    	INI_Write("B_Owner",BizzInfo[h][bOwner]);
    	INI_WriteInt("B_Lock",BizzInfo[h][bLock]);
    	INI_WriteInt("B_Owned",BizzInfo[h][bOwned]);
    	INI_WriteInt("B_Product",BizzInfo[h][bProduct]);
    	INI_WriteInt("B_Takings",BizzInfo[h][bTakings]);
    	INI_WriteInt("B_EP",BizzInfo[h][bEP]);
    	INI_WriteInt("B_Cost",BizzInfo[h][bCost]);
    	INI_WriteInt("B_Price",BizzInfo[h][bPrice]);
    	INI_WriteInt("B_Bills",BizzInfo[h][bBills]);
       	INI_WriteInt("B_SellType",BizzInfo[h][bSellType]);
    	for(new l=0;l<MAX_SELLLIST;l++)
    	{
    	    format(dir,sizeof(dir),"B_SellType_%d",l);
   	    	INI_WriteInt(dir,BizzInfo[h][bSellList][l]);
    	    format(dir,sizeof(dir),"B_SellMenu_%d",l);
   	    	INI_Write(dir,SellMenu[h][l]);
    	}
		INI_Close();
	}
	return 1;
}

