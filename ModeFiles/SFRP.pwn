CheckPlayerName(const name[])
{
	new ch[3];
	for (new i = 0; i < strlen(name); i++)
	{
		if(name[i] == '_') ch[0] ++;
		if(name[i] >= 'A' && name[i] <= 'Z') ch[1] ++;
		if((name[i] < 'A' || name[i] > 'Z') && (name[i] < 'a' || name[i] > 'z') && name[i] != '_') return 7;
	}
	if(ch[0] != 1) return 5;
	if(ch[1] != 2) return 6;
	if(name[0] < 'A' && name[0] > 'Z') return 3; // 'A'bc_Def  대문자 확인
	for (new i = 0; i < strlen(name); i++)
	{
		if(name[i] == '_')
		{
			if(name[i+1] < 'A' || name[i+1] > 'Z') return 4; // Abc_'D'ef  대문자 확인
			if(i < 2 || i > strlen(name)-3)
				return 2; // 글자수 확인
			else
				break;
		}
	}
	return 1;
}

