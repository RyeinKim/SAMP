stock CreateGPS(playerid,name[], Float:x, Float:y, Float:z)
{

    format(GPSInfo[playerid][GPS[playerid]][gName],256,"%s",name);
    GPSInfo[playerid][GPS[playerid]][gX] = x;
    GPSInfo[playerid][GPS[playerid]][gY] = y;
    GPSInfo[playerid][GPS[playerid]][gZ] = z;
    GPS[playerid]++;
    return 1;
}

stock GetGPSInfo(playerid)
{
	format(GPS_STRING,sizeof GPS_STRING,"");
    for(new i=0; i<GPS[playerid]; i++)
	{
        if(GPS[playerid]==0) format(GPS_STRING,sizeof GPS_STRING,"%s",GPSInfo[playerid][i][gName]);
        else if(GPS[playerid]==MAX_GPS-1) format(GPS_STRING,sizeof GPS_STRING,"%s%s",i,GPS_STRING,GPSInfo[playerid][i][gName]);
		else format(GPS_STRING,sizeof GPS_STRING,"%s%s\n",GPS_STRING,GPSInfo[playerid][i][gName]);
    }
    return GPS_STRING;
}

