public RemovePlayerWeapon(playerid, weaponid)
{
	new plyWeapons[13] = 0;
	new plyAmmo[13] = 0;
	for(new slot = 0; slot < 13; slot++)
	{
		new wep, ammo;
		GetPlayerWeaponData(playerid, slot, wep, ammo);
		if(wep != weaponid && ammo != 0)
		{
			GetPlayerWeaponData(playerid, slot, plyWeapons[slot], plyAmmo[slot]);
		}
	}
	ResetPlayerWeapons(playerid);
	for(new slot = 0; slot < 13; slot++)
	{
		if(plyAmmo[slot] != 0)
		{
			SafeGivePlayerWeapon(playerid, plyWeapons[slot], plyAmmo[slot]);
		}
	}
	return 1;
}

stock CheckPlayerDistanceToVehicle(Float:radi, playerid, vehicleid)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:PX,Float:PY,Float:PZ,Float:X,Float:Y,Float:Z;
		GetPlayerPos(playerid,PX,PY,PZ);
		GetVehiclePos(vehicleid, X,Y,Z);
		new Float:Distance = (X-PX)*(X-PX)+(Y-PY)*(Y-PY)+(Z-PZ)*(Z-PZ);
		if(Distance <= radi*radi)
		{
			return 1;
		}
	}
	return 0;
}
public Float:GetDistance(playerid, Float:x, Float:y)
{
	new Float:x2, Float:y2, Float:z2;
	GetPlayerPos(playerid, x2, y2, z2);
	x = x - x2;
	y = y - y2;
	return floatsqroot(x*x+y*y);
}


public LoadPos(p)
{
	if(CheckPositions(p))
	{
	    new dir[256];
	    format(dir,sizeof(dir),"Positions/%d.ini",p);
		if(INI_Exist(dir))
		{
		    INI_Open(dir,Read);
			Pos[p][Usetype]=strval(INI_Read("P_Usetype"));
			Pos[p][enterposx]=floatstr(INI_Read("P_EnterPosX"));
			Pos[p][enterposy]=floatstr(INI_Read("P_EnterPosY"));
			Pos[p][enterposz]=floatstr(INI_Read("P_EnterPosZ"));
			Pos[p][exitposx]=floatstr(INI_Read("P_ExitPosX"));
			Pos[p][exitposy]=floatstr(INI_Read("P_ExitPosY"));
			Pos[p][exitposz]=floatstr(INI_Read("P_ExitPosZ"));
			Pos[p][enterposvws]=strval(INI_Read("P_EnterPosVws"));
			Pos[p][enterposints]=strval(INI_Read("P_EnterPosInts"));
			Pos[p][exitposvws]=strval(INI_Read("P_ExitPosVws"));
			Pos[p][exitposints]=strval(INI_Read("P_ExitPosInts"));
			Pos[p][enterjob]=strval(INI_Read("P_EnterJob"));
			Pos[p][exitjob]=strval(INI_Read("P_ExitJob"));
			INI_Close();
			if(Pos[p][Usetype] != 0)
			{
				Pos[p][enterPickupId] = CreateDynamicPickup(1559, 1, Pos[p][enterposx], Pos[p][enterposy], Pos[p][enterposz],Pos[p][enterposvws], Pos[p][enterposints]);
				Pos[p][exitPickupId] = CreateDynamicPickup(1559, 1, Pos[p][exitposx], Pos[p][exitposy], Pos[p][exitposz],Pos[p][exitposvws], Pos[p][exitposints]);
			}
		}
	}
	return 1;
}

public SavePos(p)
{
	if(Pos[p][Usetype] != 0)
	{
	    new dir[256];
	    format(dir,sizeof(dir),"Positions/%d.ini",p);
	    INI_Open(dir,Write);
		INI_WriteInt("P_Usetype",Pos[p][Usetype]);
		INI_WriteFloat("P_EnterPosX",Pos[p][enterposx]);
		INI_WriteFloat("P_EnterPosY",Pos[p][enterposy]);
		INI_WriteFloat("P_EnterPosZ",Pos[p][enterposz]);
		INI_WriteFloat("P_ExitPosX",Pos[p][exitposx]);
		INI_WriteFloat("P_ExitPosY",Pos[p][exitposy]);
		INI_WriteFloat("P_ExitPosZ",Pos[p][exitposz]);
		INI_WriteInt("P_EnterPosVws",Pos[p][enterposvws]);
		INI_WriteInt("P_EnterPosInts",Pos[p][enterposints]);
		INI_WriteInt("P_ExitPosVws",Pos[p][exitposvws]);
		INI_WriteInt("P_ExitPosInts",Pos[p][exitposints]);
		INI_WriteInt("P_EnterJob",Pos[p][enterjob]);
		INI_WriteInt("P_ExitJob",Pos[p][exitjob]);
		INI_Close();
	}
	return 1;
