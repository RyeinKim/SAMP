public OnVehicleSpawn(vehicleid)
{
	if(SirenVehicle[vehicleid]==1)
	{
	    SirenVehicle[vehicleid]=0;
	    strdel(SirenURL[vehicleid],0,256);
	    DestroyObject(SirenObject[vehicleid]);
	}
	if(CarInfo[vehicleid][cCreate] != 0)
	{
	    SetVehicleHealth(vehicleid, CarInfo[vehicleid][cHealth]);
	    if(IsACop2(2, CarInfo[vehicleid][cMember]) || CarInfo[vehicleid][cMember] == 4)
	    {
			CarInfo[vehicleid][cRealHealth]=2000;
			CarInfo[vehicleid][cHealth]=2000;
			SetVehicleHealth(vehicleid, CarInfo[vehicleid][cHealth]);
		}
		else
			SetVehicleHealth(vehicleid, 1000.0);
		SetVehiclePos(vehicleid,CarInfo[vehicleid][cPosx],CarInfo[vehicleid][cPosy],CarInfo[vehicleid][cPosz]);
		ChangeVehicleColor(vehicleid,CarInfo[vehicleid][cColor],CarInfo[vehicleid][cColor2]);
		for(new i = 0;i < MAX_TUNING;i++)
		{
			AddVehicleComponent(vehicleid,CarInfo[vehicleid][cTuning][i]);
		}
		if(CarInfo[vehicleid][cPaintjob] != 999)
		{
			ChangeVehiclePaintjob(vehicleid, CarInfo[vehicleid][cPaintjob]);
		}
		SetVehicleZAngle(vehicleid,CarInfo[vehicleid][cAngle]);
		if(CarInfo[vehicleid][cHealth]) SetVehicleHealth(vehicleid, CarInfo[vehicleid][cHealth]);
		SetVehicleVirtualWorld(vehicleid,CarInfo[vehicleid][cVir]);
		SetVehicleInterior(vehicleid, CarInfo[vehicleid][cInt]);
//		}
	}
	else if(IsRentCar(vehicleid) || JCAR[49][0] <= vehicleid && JCAR[49][1])
	{
		for(new i = 0; i < M_P; i++)
		{
			if(HireCar[i] == vehicleid)
			{
				if(HireCarType[i] == 1)
				{
					DestroyVehicle(vehicleid);
					Vint[vehicleid] = 0;
				}
				else
				{
					SetVehicleVirtualWorld(vehicleid, 0);
					SetVehicleInterior(vehicleid, 0);
				}
				HireCar[i] = 0;
				Ttime[i] = 0;
			}
		}
		CarInfo[vehicleid][cLock] = 0;
	}
	if(CarLight[vehicleid] == true)
	{
		CarLight[vehicleid] = false;
		LightOnOff[vehicleid] = false;
		KillTimer(CarLightTimer[vehicleid]);
	}
