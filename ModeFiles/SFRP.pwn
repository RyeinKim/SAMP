			else if(PlayerInfo[playerid][pRespawntype] == 1)
			{
		  		SetPlayerInterior(playerid, FactionInfo[PlayerInfo[playerid][pMember]][fInterior]);
				SetPlayerVirtualWorld(playerid, FactionInfo[PlayerInfo[playerid][pMember]][fVirtualWorld]);
				SafeSetPlayerPos(playerid,FactionInfo[PlayerInfo[playerid][pMember]][fX],FactionInfo[PlayerInfo[playerid][pMember]][fY],FactionInfo[PlayerInfo[playerid][pMember]][fZ]);
				TogglePlayerControllable(playerid, 0);
				SetTimerEx("UnFreezeInfo",1000,false, "i",playerid);
				return 1;

			}
			else if(PlayerInfo[playerid][pRespawntype] == 2)
			{
	  			SetPlayerInterior(playerid,0);
				SetPlayerVirtualWorld(playerid,0);
				SafeSetPlayerPos(playerid, JobInfo[PlayerInfo[playerid][pJob]][jX], JobInfo[PlayerInfo[playerid][pJob]][jY], JobInfo[PlayerInfo[playerid][pJob]][jZ]);
				TogglePlayerControllable(playerid, 0);
				SetTimerEx("UnFreezeInfo",1000,false, "i",playerid);
                return 1;
			}
			else if(PlayerInfo[playerid][pRespawntype] == 3)
			{
				new house = PlayerInfo[playerid][pPhousekey];
				if(HouseInfo[house][hCreate]==0||PlayerInfo[playerid][pPhousekey]==999)
				{
					SafeSetPlayerPos(playerid, -1951.4602,151.0208,26.2813); //  / -1856.16,56.41,1055.19
					SetPlayerInterior(playerid, 0);
					SetPlayerVirtualWorld(playerid, 0);
					TogglePlayerControllable(playerid, 0);
					SetTimerEx("UnFreezeInfo",1000,false, "i",playerid);
					PlayerInfo[playerid][pPhousekey]=999;
				}
				SetPlayerInterior(playerid,InteriorInfo[HouseInfo[house][hInterior]][iInterior]);
				SetPlayerVirtualWorld(playerid,house+100);
				SafeSetPlayerPos(playerid, InteriorInfo[HouseInfo[house][hInterior]][iX], InteriorInfo[HouseInfo[house][hInterior]][iY], InteriorInfo[HouseInfo[house][hInterior]][iZ]);
				TogglePlayerControllable(playerid, 0);
				SetTimerEx("UnFreezeInfo",3000,false, "i",playerid);
				return 1;
			}

			if(PhoneNumbers < 10000)
			{
	    		PhoneNumbers = 10000;
	    		SavePhoneNumber(playerid);
				LoadPhoneNumber(playerid);
				return 1;
			}
			return 1;
		}
	}
	return 1;
}

