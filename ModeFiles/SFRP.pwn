public LoadSprayTags(sprayid)
{
	if(!CheckSprayTags(sprayid))
	{
 	    new dir[256];
	    format(dir,sizeof(dir),"SprayTags/%d.ini",sprayid);
	    INI_Open(dir,Write);
 		INI_WriteInt("S_Member",random(sizeof(SprayList)));
		INI_Close();
	}

	if(CheckSprayTags(sprayid))
	{
 	    new dir[256];
	    format(dir,sizeof(dir),"SprayTags/%d.ini",sprayid);
		if(INI_Exist(dir))
		{
		    INI_Open(dir,Read);
	 		SprayInfo[sprayid][sId]=strval(INI_Read("S_Member"));
			INI_Close();
			SprayInfo[sprayid][sObject] = CreateDynamicObject(SprayList[SprayInfo[sprayid][sId]][slModelid], SprayInfo[sprayid][sX], SprayInfo[sprayid][sY], SprayInfo[sprayid][sZ], SprayInfo[sprayid][sRx], SprayInfo[sprayid][sRy], SprayInfo[sprayid][sRz]);
		}
	}
	return 1;
}



public SaveSprayTags(sprayid)
{
	if(!CheckSprayTags(sprayid))
	{
 	    new dir[256];
	    format(dir,sizeof(dir),"SprayTags/%d.ini",sprayid);
	    INI_Open(dir,Write);
 		INI_WriteInt("S_Member",random(sizeof(SprayList)));
		INI_Close();
	}
	else if(CheckSprayTags(sprayid))
	{
 	    new dir[256];
	    format(dir,sizeof(dir),"SprayTags/%d.ini",sprayid);
	    INI_Open(dir,Write);
 		INI_WriteInt("S_Member",SprayInfo[sprayid][sId]);
		INI_Close();
	}
	return 1;
}

