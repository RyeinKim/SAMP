public Timer4(playerid)
{
	new Float:ppos[3];
	GetPlayerPos(playerid, ppos[0], ppos[1], ppos[2]);
	for(new i = 0; i < M_P; i++)
	{
		if(IsPlayerConnected(playerid) && Audio_IsClientConnected(i) && gAudio[i]==0)
		{
			if(PlayingBoombox[playerid][i] == 0)
			{
				if(!BoomboxInfo[playerid][icOb])
				{
					if(IsPlayerInRangeOfPoint(i, 20.0, ppos[0], ppos[1], ppos[2]))
					{
						PlayingBoombox[playerid][i] = Audio_PlayStreamed(i, BoomboxInfo[playerid][icChannel],false,true,false);
					}
				}
				else
				{
					if(IsPlayerInRangeOfPoint(i, 20.0, BoomboxInfo[playerid][icX], BoomboxInfo[playerid][icY], BoomboxInfo[playerid][icZ]))
					{
						PlayingBoombox[playerid][i] = Audio_PlayStreamed(i, BoomboxInfo[playerid][icChannel],false,true,false);
					}
				}
			}
			else
			{
				if(!BoomboxInfo[playerid][icOb])
				{
					if(!IsPlayerInRangeOfPoint(i, 20.0, ppos[0], ppos[1], ppos[2]))
					{
						Audio_Stop(i, PlayingBoombox[playerid][i]);
						PlayingBoombox[playerid][i] = 0;
						continue;
					}
				}
				else
				{
					if(!IsPlayerInRangeOfPoint(i, 20.0, BoomboxInfo[playerid][icX], BoomboxInfo[playerid][icY], BoomboxInfo[playerid][icZ]))
					{
						Audio_Stop(i, PlayingBoombox[playerid][i]);
						PlayingBoombox[playerid][i] = 0;
						continue;
					}
				}
			}
			if(!BoomboxInfo[playerid][icOb])
			{
				new vol = floatround((float(10)-GetPlayerDistancePos(i, ppos[0], ppos[1], ppos[2]))*10, floatround_round)+5;
				if(vol >= 0)
					Audio_SetVolume(i, PlayingBoombox[playerid][i], vol);
				else
					Audio_SetVolume(i, PlayingBoombox[playerid][i], 0);
			}
			else
			{
				new vol = floatround((float(10)-GetPlayerDistancePos(i, BoomboxInfo[playerid][icX], BoomboxInfo[playerid][icY], BoomboxInfo[playerid][icZ]))*10, floatround_round)+5;
				if(vol >= 0)
					Audio_SetVolume(i, PlayingBoombox[playerid][i], vol);
				else
					Audio_SetVolume(i, PlayingBoombox[playerid][i], 0);
			}
		}
	}
	return 1;
}


public GetPlayerDistancePos(playerid, Float:x2, Float:y2, Float:z2)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:x1;
		new Float:y1;
		new Float:z1;
		GetPlayerPos(playerid, x1, y1, z1);
		if(floatround(floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)), floatround_round) < 0)
			return -floatround(floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)), floatround_round);
		return floatround(floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)), floatround_round);
	}
	return 0;
}

