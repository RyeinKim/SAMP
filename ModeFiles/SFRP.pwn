stock GetClosestFurniture(playerid)
{
	new i=1;
	while(i != MAX_FURNITURE) {
	    if(IsPlayerInRangeOfPoint(playerid, 2, FurnitureInfo[i][mX], FurnitureInfo[i][mY], FurnitureInfo[i][mZ]) && FurnitureInfo[i][mLiftup] == false) {
	        return i;
		}
		i++;
	}
	return -1;
}


