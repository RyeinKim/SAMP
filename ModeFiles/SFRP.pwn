		if(PlayerInfo[playerid][pMember] == 11)
		{
			return 0;
		}
 		if(PlayerInfo[playerid][pMember] == 12)
		{
			return 0;
		}
 		if(PlayerInfo[playerid][pMember] == 13)
		{
			return 0;
		}
		else if(FactionInfo[PlayerInfo[playerid][pMember]][fCreate] == 1)
		{
			if(type == 1)
			{
				if(FactionInfo[PlayerInfo[playerid][pMember]][fType] == 3 || FactionInfo[PlayerInfo[playerid][pMember]][fType] == 4)
				return 1;
			}
			else if(type == 2)
			{
